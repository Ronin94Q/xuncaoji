  margin-top: 5rpx;
}

.platform-text {
  width: 95rpx;
  height: 23rpx;
  font-size: 24rpx;
  font-weight: 400;
  margin-right: 39rpx;
  color: rgba(170, 170, 170, 1);
}

/*添加收货地址*/

.address {
  overflow: hidden;
  margin-bottom: 20rpx;
