  opacity: 0.5;
}

.modal-dialog1 {
  width: 540rpx;
  height: 562rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 24rpx;
  overflow: hidden;
  position: fixed;
  top: 15%;
  left: 125rpx;
  z-index: 9999;
}

.modal-title1 {
  position: absolute;
  top: 54rpx;
  left: 52rpx;
  font-size: 34rpx;
  color: rgba(51, 51, 51, 1);
}

.section1 {
