.interest-zan-dialog__mask {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: rgba(0, 0, 0, 0.6);
    z-index: 9999;
  }
  
  .interest-zan-dialog__mask .zan-dialog__container {
    width: 600rpx;
    background: rgba(255, 255, 255, 1);
    border-radius: 24rpx;
    overflow: hidden;
    position: fixed;
    top: 220rpx;
    left: 75rpx;
    z-index: 9999;
  }
  
  .interest-zan-dialog__mask .cancel-img {
    width: 44rpx;
    height: 74rpx;
    position: absolute;
    right: 68rpx;
    top: 226rpx;
  }
  
