  position: relative;
  z-index: 2;
}

.view_content>.content_top:first-child {
  margin-bottom: 10rpx;
}

.content_top {
  display: flex;
  justify-content: space-between;
}

.top_left, .top_right {
  display: flex;
  align-items: center;
}

.view_whatenergy {
  color: #fff;
  font-size: 30rpx;
  font-family: 'wangte';
  letter-spacing: 4rpx;
  display: flex;
}

.view_whatenergy .text_C {
  margin-top: -3rpx;
}

