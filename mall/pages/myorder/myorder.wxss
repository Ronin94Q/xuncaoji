/* pages/myorder/myorder.wxss */

page {
  background: #f4f4f4;
  z-index: 999;
}

.content {
  width: 100%;
  height: auto;
  overflow: hidden;
}

.orderTab {
  width: 100%;
  border-bottom: 2rpx solid #f4f4f4;
  height: 80rpx;
  background-color: #fff;
  font-size: 0;
  white-space: nowrap;
}

.orderTabItem {
  width: 20%;
  height: 79rpx;
  font-size: 30rpx;
  text-align: center;
  position: relative;
  vertical-align: middle;
  display: inline-block;
}

.order-item {
  width: 55%;
  margin-left: 20%;
}

.tabTitle {
  line-height: 75rpx;
}

.active {
  color: rgba(255, 141, 18, 1);
  border-bottom: 4rpx solid rgba(255, 141, 18, 1);
}

/* 列表 */

.myOrder-wrapper {
  background: rgba(255, 255, 255, 1);
  margin-bottom: 20rpx;
  position: relative;
}

.shop {
  position: relative;
  height: 80rpx;
  line-height: 80rpx;
  border-bottom: 2rpx solid #f4f4f4;
  display: flex;
  font-size: 26rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
}

.goods-channel {
  width: 60rpx;
  height: 28rpx;
  line-height: 28rpx;
  text-align: center;
  background: rgba(255, 81, 122, 1);
  border-radius: 14px;
  font-size: 20rpx;
  color: rgba(255, 255, 255, 1);
  margin-left: 10rpx;
  margin-top: 26rpx;
}

.shop image {
  width: 32rpx;
  height: 32rpx;
  margin-left: 26rpx;
  margin-right: 13rpx;
  margin-top: 25rpx;
  border-radius: 50%;
}

.shop .title {
  color: rgba(102, 102, 102, 1);
}

.shop .state {
  color: rgba(227, 58, 89, 1);
  position: absolute;
  right: 30rpx;
}

.shops-wrap-bottom {
  padding-bottom: 40rpx;
  display: flex;
  border-bottom: 2rpx solid #f4f4f4;
}

.shops-wrap-bottom-img {
  width: 144rpx;
  height: 120rpx;
  margin-left: 30rpx;
  margin-top: 24rpx;
  border-radius: 8rpx;
}

.shops-wrap-bottom-right {
  display: flex;
  margin-left: 24rpx;
  margin-top: 20rpx;
  justify-content: start;
}

.shops-wrap-bottom-title {
  width: 310rpx;
  display: -webkit-box;
  word-break: break-all;
  text-overflow: ellipsis;
  font-size: 28rpx;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 3;
  margin-right: 95rpx;
  color: rgba(51, 51, 51, 1);
}

.shops-wrap-bottom-size {
  font-size: 24rpx;
  margin-top: 15rpx;
  color: rgba(153, 153, 153, 1);
}

.size1 {
  margin-left: 20rpx;
}

.shops-wrap-bottom-price {
  position: absolute;
  right: 30rpx;
  font-size: 24rpx;
  color: rgba(153, 153, 153, 1);
}

.price1 {
  font-size: 32rpx;
}

.shops-wrap-bottom-num {
  font-size: 24rpx;
  color: rgba(51, 51, 51, 1);
  margin-left: 88rpx;
  margin-top: 50rpx;
}

.all {
  display: flex;
  height: 80rpx;
  line-height: 80rpx;
  justify-content: flex-end;
  margin-right: 30rpx;
  font-size: 26rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(102, 102, 102, 1);
}

.all .num {
  margin-top: 2rpx;
  margin-right: 20rpx;
}

.all .price .sign {
  font-size: 24rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
}

.all .price .text {
  font-size: 32rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
}

.button {
  display: flex;
  height: 80rpx;
  justify-content: flex-end;
  margin-right: 5rpx;
}

.button text {
  display: block;
  margin-right: 20rpx;
  width: 150rpx;
  height: 56rpx;
  line-height: 56rpx;
  text-align: center;
  background: rgba(255, 255, 255, 1);
  border-radius: 28rpx;
  font-size: 26rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
}

.cancel {
  border: 2rpx solid rgba(153, 153, 153, 1);
  color: rgba(153, 153, 153, 1);
}

.return {
  border: 2rpx solid rgba(153, 153, 153, 1);
  color: rgba(153, 153, 153, 1);
}

.return-money {
  border: 2rpx solid rgba(153, 153, 153, 1);
  color: rgba(153, 153, 153, 1);
}

.pay {
  border: 2rpx solid rgba(255, 81, 122, 1);
  color: rgba(255, 81, 122, 1);
}

.myorder-bottom {
  font-size: 24rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  text-align: center;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}

/* 无订单的时候 */

.cont_count {
  height: 503rpx;
  text-align: center;
  background: rgba(255, 255, 255, 1);
}

.cont_count image {
  width: 200rpx;
  height: 200rpx;
  margin: 72rpx 274rpx;
  margin-bottom: 30rpx;
}

.cont_count .none_tab {
  font-size: 28rpx;
