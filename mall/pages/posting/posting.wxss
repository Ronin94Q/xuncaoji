/* pages/posting/posting.wxss */

page {
  background: #fff;
}

/* 普通帖 */

.ordinary_text {
  width: 100%;
  height: 176rpx;
  background-color: #fff;
  padding: 0 32rpx;
  padding-top: 30rpx;
  font-size: 28rpx;
  box-sizing: border-box;
  color: #aaa;
}

.img_wrap {
  width: 100%;
  background-color: #fff;
  padding: 0 45rpx;
  padding-bottom: 59rpx;
  display: flex;
  flex-wrap: wrap;
  padding-top: 40rpx;
  box-sizing: border-box;
}

.img_wrap image {
  width: 200rpx;
  height: 200rpx;
  border-radius: 8rpx;
  margin: 0 10rpx;
  margin-bottom: 18rpx;
}

.commentImg_wrap {
  position: relative;
}

.commentImg_wrap .close {
  width: 34rpx;
  height: 34rpx;
  position: absolute;
  top: -10rpx;
  right: -10rpx;
}

.ordinaryPosting {
  display: flex;
  justify-content: flex-end;
  margin-left: 610rpx;
  width: 109rpx;
  height: 42rpx;
}

/* 买帖 */

.buy {
  background: #fff;
}

.listItem {
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 90rpx;
  padding: 0 30rpx;
  border-bottom: 2rpx solid #f4f4f4;
  font-size: 28rpx;
  color: #aaa;
}

.listItem .listRight {
  display: flex;
  align-items: center;
}

.listItem .listRight input {
  width: 200rpx;
}

.listItem .listRight input.input1 {
  text-align: right;
}

.listItem .listRight .reduce {
  color: #333;
  margin: 0 25rpx;
}

.listItem .listRight .price {
  color: #333;
}

.listItem .listRight1 {
  display: flex;
  align-items: center;
}

.listItem .listRight1 input {
  width: 170rpx;
}

.listItem .listRight1 input.input1 {
  text-align: right;
}

.listItem .listRight1 .reduce {
  color: #333;
  margin: 0 40rpx;
}

.listItem .listRight1 .price {
  color: #333;
}

.buy_text {
  width: 100%;
  height: 305rpx;
  line-height: 40rpx;
  background-color: #fff;
  padding: 30rpx 32rpx;
  font-size: 28rpx;
  box-sizing: border-box;
  color: #aaa;
}

.buyPosting {
  display: flex;
  justify-content: flex-end;
  margin-left: 610rpx;
  width: 109rpx;
  height: 42rpx;
}

/* 卖帖 */

.sale {
  background: #fff;
}

.sale_listItem {
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 90rpx;
  padding: 0 30rpx;
  border-bottom: 2rpx solid #f4f4f4;
  font-size: 28rpx;
  color: #aaa;
  margin-bottom: 20rpx;
}

.sale_listItem .sale_listRight {
  width: 40rpx;
  height: 40rpx;
}

.sale_expect {
  padding-left: 30rpx;
  font-size: 28rpx;
  border-bottom: 2rpx solid #f4f4f4;
}

.saleListTop {
  display: flex;
  align-items: center;
  justify-content: space-between;
  width:690rpx;
  height:110rpx;
  background:rgba(255,255,255,1);
  box-shadow:0px 0px 16rpx 0px rgba(218,218,218,0.75);
  border-radius:10rpx;
  margin-top: 20rpx;
}

.saleLeft {
  display: flex;
  align-items: center;
  margin-left: 30rpx;
  font-size: 32rpx;
  color: #AAAAAA;
}

.saleLeft .textPrice {
  height: 90rpx;
  line-height: 90rpx;
  width: 360rpx;
  color: #aaa;
  margin-left: 20rpx;
}
.saleRight {
  font-weight:bold;
  color:rgba(183,144,91,1);
  margin-right: 31rpx;
  font-size: 28rpx;
}

.saleLeave {
  font-size: 26rpx;
  margin: 30rpx 0;
  color: #999;
  width: 620rpx;
}

.saleYear {
  font-size: 26rpx;
  margin: 30rpx 0;
  margin-top: 0;
  color: #999;
}

.saleYear text {
  color: #ff2644;
}

.saleText {
  font-size: 26rpx;
  line-height: 40rpx;
  margin: 30rpx 0;
  margin-top: 0;
  color: #ff2644;
  width: 620rpx;
}

.replyChoose {
  font-size: 26rpx;
  margin: 30rpx 0;
  margin-top: 0;
  width: 163rpx;
  height: 54rpx;
  border:1px solid rgba(183,144,91,1);
  text-align: center;
  line-height: 54rpx;
  color: rgba(183,144,91,1);
  border-radius: 27rpx;
}

.sale_text {
  width: 690rpx;
  height: 340rpx;
  margin-left: 30rpx;
  line-height: 40rpx;
  background:rgba(244,244,244,1);
  padding: 30rpx 32rpx;
  font-size: 28rpx;
  box-sizing: border-box;
  color: #aaa;
  border-radius:8rpx;
}
.jia{
  color: #333333;
}
.salePosting {
  display: flex;
  justify-content: flex-end;
  margin-left: 610rpx;
  width: 109rpx;
  height: 42rpx;
}
.msgText{
  height: 50rpx;
  line-height: 50rpx;
  background: #fff;
  text-align: right;
  font-size: 26rpx;
  color: #ff2644;
  padding-right: 30rpx;
  margin-bottom: 20rpx;
}
