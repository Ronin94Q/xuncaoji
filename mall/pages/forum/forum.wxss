  height: 44rpx;
  margin-top: 30rpx;
  align-items: center;
}

.content_time_left {
  display: flex;
  align-items: center;
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333;
}

.content_time_left .content_time_item {
  color: #999;
  margin-right: 24rpx;
}

.content_time_right {
  display: flex;
  align-items: center;
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 500;
}

.content_time_img {
  display: flex;
  align-items: center;
  margin-right: 15rpx;
}

.content_time_imgs {
  display: flex;
  align-items: center;
}

.content_time_right .collect {
  width: 30rpx;
  height: 30rpx;
  margin-right: 12rpx;
}

.content_time_imgs .content_time_text {
  color: #666;
}

.content_time_right .comment {
  width: 30rpx;
  height: 30rpx;
  margin-right: 12rpx;
}

.content_comment {
  width: 690rpx;
  background: rgba(244, 244, 244, 1);
  border-radius: 8rpx;
  margin: 30rpx 0;
}

.content_comment_text {
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #333;
  margin: 0 20rpx;
  line-height: 1.5;
}

.content_comment_text .item {
  font-weight: normal;
  color: #666;
}

.all_comment {
  color: #999;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 500;
  margin-left: 20rpx;
}

/* 删除帖子弹框 */

.del-zan-dialog__mask {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.6);
  z-index: 97;
}

.del-zan-dialog__mask .zan-dialog__container1 {
  width: 630rpx;
  height: 365rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 24rpx;
  position: absolute;
  right: 112rpx;
  top: 300rpx;
  left: 62rpx;
  text-align: center;
  z-index: 97;
}

.del-zan-dialog__mask .zan-dialog__container1 .msg {
  margin: 44rpx auto;
  margin-bottom: 20rpx;
  font-size: 34rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: rgba(34, 34, 34, 1);
}

.del-zan-dialog__mask .zan-dialog__container1 .msg_content {
  margin: 0 auto;
  width: 358rpx;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333;
  line-height: 40rpx;
}

.del-zan-dialog__mask .zan-dialog__container1 .btn {
  display: flex;
  justify-content: space-between;
  width: 550rpx;
  margin-left: 40rpx;
  margin-top: 63rpx;
  font-size: 32rpx;
  font-family: PingFang SC;
  font-weight: 400;
}

.del-zan-dialog__mask .zan-dialog__container1 .cancelDel {
  width: 264rpx;
  margin-right: 22rpx;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 40rpx;
  border: 2rpx solid #ff2644;
  color: #ff2644;
}

.del-zan-dialog__mask .zan-dialog__container1 .confirmDel {
  width: 264rpx;
