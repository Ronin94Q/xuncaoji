  display: flex;
  justify-content: space-around;
}

.content-wrapper1 .share-button {
  background: rgba(255, 255, 255, 1);
  width: 198rpx;
  border-radius: 0;
}

button::after {
  border: none;
}

.content-wrapper1 .photo {
