}

.cashback-detail .list .text1 {
  font-size: 28rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(170, 170, 170, 1);
}

.cashback-detail .list .refund {
  width: 113rpx;
