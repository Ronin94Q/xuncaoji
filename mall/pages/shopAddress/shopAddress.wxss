.wraper .listItems {
  height: 90rpx;
  line-height: 90rpx;
  position: relative;
  display: flex;
  border-bottom: 2rpx solid #f4f4f4;
  background: rgba(255, 255, 255, 1);
}

.wraper .listItems .listLeft {
  margin-right: 15rpx;
  margin-left: 30rpx;
  font-size: 28rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
}
.wraper .listItems .listRight {
  font-size: 28rpx;
  margin-top: 20rpx;
  width: 550rpx;
}
