  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  background: rgba(189, 189, 189, 1);
  border-radius: 25px;
  font-size: 24rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
}

.num-item2 {
  position: absolute;
  right: 40rpx;
  bottom: 30rpx;
  width: 168rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  background: rgba(189, 189, 189, 1);
  border-radius: 25px;
  font-size: 24rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
}

.num-item text {
  width: 60rpx;
  line-height: 50rpx;
  text-align: center;
}

.num-item input {
  width: 50rpx;
  height: 44rpx;
  margin: 0 auto;
  text-align: center;
  font-size: 32rpx;
  border-left: 2rpx solid #ccc;
  border-right: 2rpx solid #ccc;
}
.detail-bottom {
  font-size: 24rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  text-align: center;
  margin-top: 20rpx;
  margin-bottom: 40rpx;
}
/* 底部 */
.store-total {
  display: flex;
  line-height: 90rpx;
  font-size: 30rpx;
  height: 90rpx;
  background: #fff;
  border-top: 2rpx solid #f4f4f4;
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  z-index: 999;
}

.store-total-box {
  margin-left: 25rpx;
  margin-right: 30rpx;
  width: 30rpx;
  height: 30rpx;
}

.store-total-expenses {
  margin-left: 60rpx;
  margin-right: 20rpx;
  font-size: 24rpx;
  color: rgba(136, 136, 136, 1);
}

.store-total-btn {
  position: absolute;
  right: 0;
  bottom: 0;
  width: 180rpx;
  text-align: center;
  color: #fff;
}

.store-total-num {
  font-size: 28rpx;
  color: #e4405e;
}

.store-total-num-text {
  font-size: 24rpx;
}
/* 删除按钮 */

/* 弹框 */

.zan-dialog__mask {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  display: none;
  z-index: 99;
  background: rgba(0, 0, 0, 0.4);
}

.zan-dialog__container {
  transform: translateY(300%);
  transition: all 0.00001s ease;
  width: 540rpx;
  height: 282rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 24rpx;
  overflow: hidden;
  position: fixed;
  bottom: 550rpx;
  left: 110rpx;
  z-index: 9999;
}

.zan-dialog--show .zan-dialog__container {
  transform: translateY(0);
}

.zan-dialog--show .zan-dialog__mask {
  display: block;
}
/* 弹框内容 */
.content-mask {
  text-align: center;
  overflow: hidden;
}

.content-title {
  margin-top: 44rpx;
  margin-bottom: 10rpx;
  font-size: 34rpx;
  font-weight: bold;
  color: rgba(34, 34, 34, 1);
}

.content-container {
  width: 450rpx;
  height: 66rpx;
  margin-left: 55rpx;
  margin-bottom: 35rpx;
  font-size: 30rpx;
  text-align: center;
  color: rgba(51, 51, 51, 1);
}

.content-button {
  display: flex;
  height: 66rpx;
  border-top: 2rpx solid #eaeaea;
}

.reject {
  border-right: 2rpx solid #eaeaea;
  width: 270rpx;
  height: 75rpx;
  line-height: 75rpx;
  font-size: 34rpx;
  color: rgba(51, 51, 51, 1);
}

.comfirm {
  height: 66rpx;
  line-height: 66rpx;
  font-size: 34rpx;
  text-align: center;
  width: 270rpx;
  background: rgba(255, 255, 255, 1);
  color: #FF6417;
}
/* 轮播展示组件 */
.swiper-show{
  position: fixed;
