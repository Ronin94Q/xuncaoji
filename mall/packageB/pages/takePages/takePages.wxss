  margin-right: 12rpx;
}

.goodsName {
  width: 440rpx;
  text-align: left;
  font-size: 32rpx;
  color: #333;
  font-family: PingFang SC;
  font-weight: bold;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  line-clamp: 1;
  -webkit-box-orient: vertical;
}

.wrap {
  margin: 0 auto;
  margin-top: 13rpx;
  width: 690rpx;
  background: #fff;
  border-radius: 20rpx;
}

.tp-title {
  height: 89rpx;
  line-height: 89rpx;
  margin-left: 30rpx;
  font-size: 32rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #333;
}

.tp-items {
  width: 100%;
  display: flex;
  padding-left: 30rpx;
  margin-bottom: 40rpx;
  box-sizing: border-box;
  background-color: #fff;
  border-radius: 20rpx;
}

.tp-getmore {
  text-align: right;
  width: 540rpx;
  height: 80rpx;
  line-height: 80rpx;
  color: #333;
  font-size: 24rpx;
  border-top: 1rpx solid #e7e7e7;
  margin-left: 120rpx;
}

/* 评价 */

.tp-evaluate {
  margin: 0 auto;
  margin-top: 20rpx;
  width: 690rpx;
  background: #fff;
  border-radius: 20rpx;
}

.tp-evaluate-title {
  height: 89rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin: 0 30rpx;
  font-size: 32rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #333;
}

.tp-tltle-right {
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #333;
}

.tp-evaluate-content {
  display: flex;
  margin-top: 20rpx;
}

.tp-evaluate-img {
  background-image: url('https://xuncaoji.net/_download/img/flashPay/other_ren.png');
  background-size: 100% 100%;
  background-repeat: no-repeat;
  width: 70rpx;
  height: 70rpx;
  margin-left: 30rpx;
  margin-right: 20rpx;
}

.tp-evaluate-img image {
  width: 70rpx;
  height: 70rpx;
}

.tp-evaluate-right {
  width: 540rpx;
}

.tp-evaluate-name {
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 26rpx;
  color: #333;
}

.tp-storeGrade {
  display: flex;
  align-items: center;
}

.tp-date-spec {
  display: flex;
  font-size: 24rpx;
  color: #999;
  margin-top: 13rpx;
}

.tp-date {
  margin-right: 22rpx;
}

.tp-evaluate-text {
  font-size: 28rpx;
  margin-top: 35rpx;
  word-break: break-all;
  color: #333;
  line-height: 36rpx;
}

.evaluate-list-scroll {
  display: flex;
  width: 100%;
  box-sizing: border-box;
  margin: 40rpx 0;
}

.evaluate-list-img {
  display: inline-block;
  width: 174rpx;
  height: 174rpx;
  border-radius: 8rpx;
  margin-right: 7rpx;
  background-image: url('https://xuncaoji.net/_download/img/icon/zhanw_tb_n.png');
  background-size: 100% 100%;
  background-repeat: no-repeat;
}

.evaluate-list-img image {
  width: 174rpx;
  height: 174rpx;
  border-radius: 8rpx;
}
