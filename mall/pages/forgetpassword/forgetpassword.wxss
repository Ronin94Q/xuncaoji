page {
  background: #f4f4f4;
}

.wraper .listItems {
  height: 90rpx;
  line-height: 90rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  border-bottom: 2rpx solid #f4f4f4;
  background: rgba(255, 255, 255, 1);
}

.wraper .listItems .listLeft {
  margin-right: 25rpx;
  margin-left: 30rpx;
  font-size: 28rpx;
  font-family: PingFang-SC-Medium;
  color: rgba(51, 51, 51, 1);
}

.wraper .listItems .listRight {
  font-size: 28rpx;
  width: 320rpx;
}

.code {
  position: absolute;
  right: 30rpx;
  top: 106rpx;
  font-size: 26rpx;
  font-family: PingFang-SC-Medium;
  background: rgba(255, 255, 255, 1);
}

button[disabled] {
  background: rgba(255, 255, 255, 1) !important;
  border: none !important;
  border-radius: 0 !important;
}

button::after {
  border: none;
}

/* 保存 */

.btn {
  width: 690rpx;
  height: 80rpx;
  background: #ff6417;
  border-radius: 40rpx;
  font-size: 32rpx;
  margin: 0 auto;
  margin-top: 70rpx;
  color: rgba(255, 255, 255, 1);
}

.btn::after {
  border: none;
}
