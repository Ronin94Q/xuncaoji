/* packageB/pages/nearbyStore/nearbyStore.wxss */

page {
  background: #f4f4f4;
}

/* 搜索 */

.index-wrap {
  position: fixed;
  top: 0;
  z-index: 9999;
  background: #ff2644;
}

/*搜索框*/

.section-wrapper {
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 28rpx;
  padding: 0 30rpx;
  width: 690rpx;
  height: 88rpx;
}

.section {
  line-height: 60rpx;
  width: 478rpx;
  height: 60rpx;
  display: flex;
  border: 1rpx solid rgba(238, 238, 238, 1);
  background: #fff;
  font-weight: 400;
  border-radius: 30rpx;
}

.section-img {
  width: 26rpx;
  height: 26rpx;
  margin: 17rpx 22rpx;
}

.input {
  color: #aaa;
}

.section1 {
  line-height: 60rpx;
  width: 192rpx;
  height: 60rpx;
  display: flex;
  background: rgba(255, 255, 255, 0.2);
  font-weight: 400;
  border-radius: 30rpx;
}

.section-img1 {
  width: 30rpx;
  height: 29rpx;
  margin: 17rpx 22rpx;
}

.input1 {
  color: #fff;
}

/* 顶部导航 */

.nav {
  width: 750rpx;
  height: 90rpx;
  line-height: 90rpx;
  display: flex;
  justify-content: space-between;
  box-sizing: border-box;
  font-size: 28rpx;
  white-space: nowrap;
  padding: 0 30rpx;
  padding-right: 50rpx;
}

.nav-box {
  display: inline-block;
  position: relative;
}

.nav-item {
  text-align: center;
  color: #fff;
}

.nav-img {
  position: absolute;
  right: -20rpx;
  top: 42rpx;
  width: 16rpx;
  height: 8rpx;
}

/* 商家 */

.wrap {
  margin-left: 30rpx;
  width: 690rpx;
  box-sizing: border-box;
  margin-top: 196rpx;
}

.items {
  width: 100%;
  height: 230rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 24rpx;
  display: flex;
  padding-left: 30rpx;
  padding-right: 20rpx;
  margin-bottom: 20rpx;
  box-sizing: border-box;
  background-color: #fff;
}

.img_wrap {
  background-image: url('https://xuncaoji.net/_download/img/icon/zhanw_tb_n.png');
  background-size: 100% 100%;
  background-repeat: no-repeat;
  width: 170rpx;
  height: 170rpx;
  margin-right: 30rpx;
  margin-top: 30rpx;
  border-radius: 20rpx;
}

.img_wrap image {
  width: 170rpx;
  height: 170rpx;
  border-radius: 20rpx;
}

.goodsDetail {
  height: 100%;
  padding-bottom: 30rpx;
  box-sizing: border-box;
}

.goodsName {
  width: 440rpx;
  text-align: left;
  margin-top: 40rpx;
  font-size: 32rpx;
  color: #333;
  font-family: PingFang SC;
  font-weight: bold;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  line-clamp: 1;
  -webkit-box-orient: vertical;
}

.goodsName1 {
  width: 440rpx;
  text-align: left;
  margin-top: 30rpx;
  font-size: 32rpx;
  color: #333;
  font-family: PingFang SC;
  font-weight: bold;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  line-clamp: 1;
  -webkit-box-orient: vertical;
}

.storeGrade {
  display: flex;
  align-items: center;
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #999;
  margin-top: 20rpx;
}

.re-storeGrade {
  display: flex;
  align-items: center;
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #999;
  margin-top: 10rpx;
}

.goodsBottom {
  display: flex;
  justify-content: space-between;
}

.storeText text {
  font-weight: normal;
}

.stars image {
  width: 24rpx;
  height: 22rpx;
  margin-right: 4rpx;
}

.takeoutT {
  margin-top: 10rpx;
  color: #ff2644;
  font-size: 24rpx;
}

.takeoutT text:nth-child(1) {
  font-weight: bold;
}

.takeoutT text:nth-child(2) {
  font-weight: bold;
  font-size: 36rpx;
}

.goods-text {
  font-size: 24rpx;
  color: #ff2644;
  margin-top: 5rpx;
}

.takeoutName {
  color: #666;
  font-size: 24rpx;
  margin-top: 10rpx;
}

.takeoutImg {
  margin-top: 10rpx;
  display: flex;
}

.takeoutF {
  margin-right: 10rpx;
  position: relative;
  width: 75rpx;
  height: 32rpx;
}

.takeoutImg image {
  width: 75rpx;
  height: 32rpx;
}

.takeoutText {
  font-size: 22rpx;
  color: #ff2644;
  position: absolute;
  top: 3rpx;
  left: 15rpx;
}

.goods-right {
  display: flex;
  align-items: center;
  flex-direction: column;
  margin-top: 10rpx;
}

.goods-m {
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #999;
}

/* 筛选 */

.choose {
  width: 750rpx;
  position: fixed;
  background-color: #fff;
  top: 176rpx;
  z-index: 999;
}

.tab {
  display: flex;
  justify-content: space-between;
  padding-left: 30rpx;
  padding-right: 30rpx;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333;
  height: 80rpx;
  align-items: center;
}

.choose-active {
  color: #ff2644;
}

.choose_img {
  width: 32rpx;
  height: 32rpx;
}

.get-zan-dialog__mask {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.6);
  z-index: 99;
}
