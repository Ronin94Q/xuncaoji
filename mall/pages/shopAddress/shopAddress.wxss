.listItems-item {
  height: 90rpx;
  line-height: 90rpx;
  display: flex;
  margin-top: 20rpx;
  margin-bottom: 70rpx;
  border-bottom: 2rpx solid #f4f4f4;
  background: rgba(255, 255, 255, 1);
}

.listItems-item .listLeft {
  margin-right: 420rpx;
