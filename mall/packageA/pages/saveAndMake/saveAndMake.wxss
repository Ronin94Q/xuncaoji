/* packageA/pages/saveAndMake/saveAndMake.wxss */

page {
  background: #f1f1f1;
}

/*导航栏*/

.content_top {
  width: 100%;
  background: #fff;
  position: fixed;
  top: 0rpx;
  height: 80rpx;
  line-height: 72rpx;
  border-bottom: 2rpx solid #f4f4f4;
  z-index: 97;
}

.nav {
  box-sizing: border-box;
  font-size: 30rpx;
  white-space: nowrap;
}

.nav-box {
  display: inline-block;
  width: 375rpx;
}

.nav-item {
  text-align: center;
  color: #555;
}

.nav-item.active {
  color: #ff2644;
}

.nav-item.active1 {
  width: 40rpx;
  height: 6rpx;
  background: #ff2644;
  margin-left: 160rpx;
}

/* 信息 */

.notStore {
 color: #333;
  font-size: 28rpx;
  text-align: center;
  margin: 0 auto;
  margin-top: 160rpx;
}

.notStore image {
  width: 380rpx;
  height: 380rpx;
}


.saveAndmake-wrap {
  margin-top: 110rpx;
}

.saveAndmake-list {
  width: 690rpx;
  max-height: 565rpx;
  background: #fff;
  border-radius: 20rpx;
  margin: 0 auto;
  margin-bottom: 20rpx;
}

.saveAndmake-top {
  display: flex;
  justify-content: space-between;
}

.clock {
  display: flex;
  align-items: center;
  margin-left: 24rpx;
  margin-top: 10rpx;
  font-size: 26rpx;
  color: #666;
}

.clock image {
  width: 23rpx;
  height: 23rpx;
  margin-right: 10rpx;
}

.sign {
  width: 137rpx;
  height: 50rpx;
}

.goods {
  height: 170rpx;
  display: flex;
  align-items: center;
  font-size: 24rpx;
  color: #999;
}

.goodsImg {
  width: 110rpx;
  height: 110rpx;
  border-radius: 4rpx;
  margin: 30rpx;
}

.goods_middle {
  width: 490rpx;
  margin: 30rpx;
  margin-top: 25rpx;
  margin-left: 0;
}

.goodsName {
  font-size: 26rpx;
  margin-bottom: 20rpx;
  color: #333;
  display: -webkit-box;
  word-break: break-all;
  text-overflow: ellipsis;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
}

.saveAndmakePrice {
  display: flex;
  height: 60rpx;
  border-bottom: 2rpx solid #e7e7e7;
}

.salePrice {
  display: flex;
  margin: 0 24rpx;
  margin-right: 10rpx;
  font-size: 22rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #b7905b;
}

.salePrice1 {
  width: 54rpx;
  height: 32rpx;
  line-height: 32rpx;
  text-align: center;
  background: #b7905b;
  border-radius: 4rpx;
  font-size: 20rpx;
  color: #fff;
  margin-left: 16rpx;
}

.salePrice1.weiyue {
  width: 74rpx;
}

.saveAndmakeText {
  width: 610rpx;
  height: 60rpx;
  font-size: 24rpx;
  color: #ff2644;
  line-height: 36rpx;
  margin-left: 36rpx;
  margin-top: 30rpx;
}

.content {
  display: flex;
  margin-top: 30rpx;
  margin-left: 30rpx;
  margin-right: 30rpx;
  align-items: center;
}

.content_left {
  display: flex;
  align-items: center;
  font-size: 28rpx;
  margin-right: 20rpx;
}

.content_left image {
  border-radius: 50%;
  width: 60rpx;
  height: 60rpx;
}

.nickname {
  margin-bottom: 10rpx;
  font-size: 22rpx;
  color: #999;
}

.message {
  color: #333;
  font-family: PingFang SC;
  font-size: 24rpx;
  font-weight: 400;
}

.saveAndmakeBtn {
  display: flex;
  justify-content: flex-end;
  height: 120rpx;
  align-items: center;
  margin-right: 30rpx;
  font-size: 26rpx;
  color: #333;
}

.modifyMsg {
  border-radius: 40rpx;
  border: 1rpx solid #999;
  width: 210rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
}

.cancel {
  border-radius: 40rpx;
  border: 1rpx solid #999;
  width: 172rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  margin-left: 20rpx;
}

.editPrice {
  border-radius: 40rpx;
  border: 1rpx solid #ff2644;
  color: #ff2644;
  width: 172rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  margin-left: 20rpx;
}

.editPrice1 {
  border-radius: 40rpx;
  border: 1rpx solid #ff2644;
  color: #ff2644;
  width: 210rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  margin-left: 20rpx;
}

.cancelFinish {
  border-radius: 40rpx;
  background: #e7e7e7;
  color: #fff;
  width: 172rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  margin-left: 20rpx;
}

.editPriceFinish {
  border-radius: 40rpx;
  background: #e7e7e7;
  color: #fff;
  width: 172rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  margin-left: 20rpx;
}

/* 交易关闭弹框 */

.yudialog__mask {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.6);
  z-index: 9999;
}

.yudialog__mask .dialog__container {
  background: rgba(255, 255, 255, 1);
  height: 395rpx;
  width: 600rpx;
  overflow: hidden;
  position: fixed;
  z-index: 9999;
  border-radius: 30rpx;
  top: 260rpx;
  left: 75rpx;
}

.yudialog__mask .cancelImg {
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  right: 55rpx;
  top: 50rpx;
}

.yudialog__mask .dialog__container .tishi {
  text-align: center;
  margin-top: 46rpx;
  margin-bottom: 35rpx;
  font-size: 36rpx;
  color: rgba(51, 51, 51, 1);
  margin-left: 50rpx;
}

.yudialog__mask .dialog__container .order {
  font-size: 30rpx;
  color: rgba(51, 51, 51, 1);
}

.yudialog__mask .order_one {
  font-size: 28rpx;
  color: #333;
  margin-left: 50rpx;
}

.yudialog__mask .order_two {
  font-size: 28rpx;
  color: #333;
  margin-left: 50rpx;
}

.order_three {
  font-size: 28rpx;
  color: #333;
  margin-left: 50rpx;
  text-align: center;
}

.yudialog__mask .order_box1 {
  width: 240rpx;
  height: 76rpx;
  border: 1rpx solid rgba(255, 38, 68, 1);
  border-radius: 38rpx;
  text-align: center;
  font-size: 32rpx;
  color: rgba(255, 38, 68, 1);
  line-height: 76rpx;
}

.yudialog__mask .order_box2 {
  width: 240rpx;
  height: 76rpx;
  background: #ff2644;
  border-radius: 38rpx;
  text-align: center;
  font-size: 32rpx;
  color: #fff;
  line-height: 76rpx;
}

.yudialog__mask .order_box {
  display: flex;
  justify-content: space-between;
  margin: 50rpx;
}

.nothing {
  width: 750rpx;
  height: 320rpx;
  margin-top: 150rpx;
}
