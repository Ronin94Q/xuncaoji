}

.section-list-img {
  width: 76rpx;
  height: 76rpx;
  border-radius: 50%;
  margin-bottom: 20rpx;
}

.section-list-img1 {
  width: 76rpx;
  height: 76rpx;
  border-radius: 50%;
  margin-bottom: 10rpx;
}

.section-list-img1 image {
  width: 76rpx;
  height: 76rpx;
  border-radius: 50%;
}

.section-list-img image {
  width: 86rpx;
  height: 86rpx;
  border-radius: 50%;
}

.section-list-text {
  font-size: 24rpx;
  color: #666;
}

.mokuai_title {
  font-weight: bold;
  margin-top: 26rpx;
  height: 71rpx;
  line-height: 71rpx;
  font-size: 32rpx;
  font-family: PingFang SC;
  color: rgba(51, 51, 51, 1);
  padding-left: 50rpx;
}

.mokuai_value {
  display: flex;
  justify-content: space-between;
