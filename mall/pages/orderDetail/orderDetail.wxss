/* pages/orderDetail/orderDetail.wxss */

page {
  background: #f4f4f4;
}

/* 订单状态 */

.orderDetail-wrapper {
  overflow: hidden;
}

.orderDetail {
  overflow: hidden;
  background: rgb(255, 255, 255);
  margin-bottom: 20rpx;
}

.orderDetail .top {
  position: relative;
  width: 690rpx;
  height: 160rpx;
  background: linear-gradient(65deg, #ff8d12 0%, #ffaa4d 100%);
  border-radius: 10rpx;
  margin-left: 30rpx;
  margin-top: 30rpx;
  display: flex;
}

.orderDetail .top .text:nth-child(1) text {
  display: block;
  margin-left: 52rpx;
  margin-top: 64rpx;
  font-size: 34rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: rgba(255, 255, 255, 1);
}

.orderDetail .top .text .text1 {
  display: block;
  margin-left: 52rpx;
  margin-top: 30rpx;
  font-size: 34rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: rgba(255, 255, 255, 1);
}

.orderDetail .top .text .text2 {
  display: block;
  margin-left: 52rpx;
  margin-top: 20rpx;
  font-size: 28rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
}

.orderDetail .top image {
  width: 80rpx;
  height: 80rpx;
  position: absolute;
  right: 52rpx;
  bottom: 40rpx;
}

/*平台保证*/

.platform {
  background: #fff;
  overflow: hidden;
}

.platform-list {
  height: 80rpx;
  line-height: 80rpx;
  display: flex;
  padding: 0 30rpx;
}

.platform-list-img {
  width: 24rpx;
  height: 24rpx;
  margin-right: 8rpx;
  margin-top: 2rpx;
  vertical-align: middle;
}

.platform-text {
  width: 95rpx;
  height: 23rpx;
  font-size: 24rpx;
  font-weight: 400;
  margin-right: 39rpx;
  color: rgba(170, 170, 170, 1);
}

/*添加收货地址*/

.address {
  overflow: hidden;
  position: relative;
  background: rgba(255, 255, 255, 1);
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 10rpx 30rpx;
  margin-bottom: 20rpx;
}

.address-img image {
  margin-right: 20rpx;
  width: 40rpx;
  height: 40rpx;
}

.address-text {
  font-size: 28rpx;
  color: rgba(51, 51, 51, 1);
}

.address-right-img image {
  width: 56rpx;
  height: 56rpx;
}

/*店铺商品*/

