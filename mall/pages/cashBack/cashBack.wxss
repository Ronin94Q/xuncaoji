.stop-zan-dialog__mask {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.6);
  z-index: 9999;
}

.stop-zan-dialog__container {
  width: 540rpx;
  height: 282rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 24rpx;
  overflow: hidden;
  position: fixed;
  top: 300rpx;
  left: 110rpx;
  z-index: 9999;
}

/* 弹框内容 */

.stop-content {
  text-align: center;
  overflow: hidden;
}

.stop-content-title {
  margin-top: 44rpx;
  margin-bottom: 10rpx;
  font-size: 34rpx;
  font-weight: bold;
  color: rgba(34, 34, 34, 1);
}

.stopzero {
  border: 2rpx solid #ff2644;
  color: #ff2644;
  width: 270rpx;
  padding: 0 5rpx;
  height: 60rpx;
  margin-right: 50rpx;
  margin-top: 20rpx;
  border-radius: 28rpx;
  line-height: 56rpx;
  font-size: 26rpx;
  background: #fff;
}

.stopzero[disabled] {
  margin-right: 0 !important;
  color: #ff2644 !important;
  border: none !important;
  background: #fff !important;
}

button::after {
  border: none;
}

.stop-content-container {
  width: 357rpx;
  height: 66rpx;
  margin-left: 100rpx;
  margin-bottom: 35rpx;
  font-size: 30rpx;
  text-align: center;
  color: rgba(51, 51, 51, 1);
}

.stop-content-button {
  display: flex;
  height: 100rpx;
  border-top: 2rpx solid #eaeaea;
}

.stop-reject {
  border-right: 2rpx solid #eaeaea;
  width: 270rpx;
  height: 75rpx;
  line-height: 75rpx;
  font-size: 34rpx;
  color: #ff2644;
}

.stop-comfirm {
  height: 66rpx;
  line-height: 66rpx;
  font-size: 34rpx;
  text-align: center;
  width: 270rpx;
  background: rgba(255, 255, 255, 1);
  color: rgba(51, 51, 51, 1);
}

.stop-comfirm::after {
  border: none;
}
