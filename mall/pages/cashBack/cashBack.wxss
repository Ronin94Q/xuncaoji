/* pages/cashBack/cashBack.wxss */

page {
  background: #f4f4f4;
}

.cashback-wrapper {
  background: rgb(255, 255, 255);
  margin-bottom: 20rpx;
  overflow: hidden;
}

.stop-zero {
  width: 150rpx;
  height: 46rpx;
  border: 2rpx solid rgba(255, 38, 68, 1);
  border-radius: 23rpx;
  margin-top: 20rpx;
  margin-left: 50%;
  font-size: 26rpx;
  line-height: 46rpx;
  text-align: center;
  font-family: PingFang SC;
  font-weight: 500;
  color: rgba(255, 38, 68, 1);
}

.cashback-message .title {
  display: flex;
  width: 750rpx;
  height: 72rpx;
  line-height: 72rpx;
  font-size: 28rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  margin-left: 30rpx;
}

.cashback-message .title text:nth-child(1) {
  display: block;
  margin-top: 23rpx;
  height: 30rpx;
  width: 6rpx;
  background: rgba(60, 213, 139, 1);
  border-radius: 3rpx;
}

.cashback-message .title text:nth-child(2) {
  padding-left: 20rpx;
}

/* 商品信息 */

.shops-wrap-bottom {
  padding-bottom: 20rpx;
  display: flex;
}

.shops-wrap-bottom-img {
  width: 122rpx;
  height: 120rpx;
  margin-left: 30rpx;
  margin-top: 24rpx;
  border-radius: 8rpx;
}

.shops-wrap-bottom-right {
  position: relative;
  display: flex;
  margin: 0 30rpx;
  margin-top: 20rpx;
  justify-content: space-between;
  width: 570rpx;
}

.shops-wrap-bottom-title {
  width: 310rpx;
  height: 79rpx;
  display: -webkit-box;
  word-break: break-all;
  text-overflow: ellipsis;
  font-size: 28rpx;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  margin-right: 95rpx;
  color: rgba(51, 51, 51, 1);
}

.shops-wrap-bottom-size {
  font-size: 24rpx;
  margin-top: 15rpx;
  color: rgba(153, 153, 153, 1);
}

.size1 {
  margin-left: 20rpx;
}

.shops-wrap-bottom-price {
  font-size: 24rpx;
  color: rgba(153, 153, 153, 1);
}

.price1 {
  font-size: 28rpx;
}

.price2 {
  position: absolute;
  right: 290rpx;
  font-size: 28rpx;
  color: #666;
}

.shops-wrap-bottom-num {
  color: rgba(153, 153, 153, 1);
  position: absolute;
  right: 0rpx;
  top: 83rpx;
}

.shops-wrap-bottom-num text:nth-child(1) {
  padding-right: 8rpx;
  font-size: 28rpx;
}

.shops-wrap-bottom-num text:nth-child(2) {
  padding-right: 2rpx;
  font-size: 26rpx;
}

