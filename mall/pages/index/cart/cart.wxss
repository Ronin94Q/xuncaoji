/* 弹框 */

.zan-dialog__mask {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.6);
  z-index: 9999;
}

.zan-dialog__container {
  width: 600rpx;
  height: 300rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 24rpx;
  overflow: hidden;
  position: fixed;
  top: 300rpx;
  left: 70rpx;
  z-index: 9999;
}

/* 弹框内容 */

.content {
  text-align: center;
  overflow: hidden;
}

.content-title {
  margin-top: 44rpx;
  margin-bottom: 10rpx;
  font-size: 34rpx;
  font-weight: bold;
  color: rgba(34, 34, 34, 1);
}

.content-container {
  width: 372rpx;
  height: 66rpx;
  margin-left: 100rpx;
  margin-bottom: 35rpx;
  font-size: 30rpx;
  text-align: center;
  color: rgba(51, 51, 51, 1);
}

.content-button {
  display: flex;
  margin-left: 50rpx;
}

.reject {
  width: 240rpx;
  height: 76rpx;
  line-height: 76rpx;
  border: 1rpx solid rgba(255, 38, 68, 1);
  border-radius: 38rpx;
  font-size: 32rpx;
  color: #ff2644;
  text-align: center;
  margin-right: 20rpx;
}

.comfirm {
  width: 240rpx;
  height: 76rpx;
  line-height: 76rpx;
  text-align: center;
  background: #ff2644;
  border-radius: 38rpx;
  font-size: 32rpx;
  color: #fff;
}

.comfirm::after {
  border: none;
}

/* 轮播展示组件 */

.swiper-show {
  position: fixed;
  top: 24rpx;
  left: 16rpx;
  z-index: 9999;
}

.applyStatus {
  height: 101%;
  width: 100%;
  background: rgba(0, 0, 0, 0.6);
  z-index: 2;
  position: absolute;
}

.againCalculate-wrap {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.calculateInvalid {
  margin-top: 89rpx;
  font-size: 32rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
}

.againCalculate {
  margin-top: 29rpx;
  text-align: center;
  line-height: 50rpx;
  width: 168rpx;
  height: 50rpx;
  background: rgba(255, 38, 68, 1);
  border-radius: 25rpx;
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
}

.addCart-wrap {
  display: flex;
  position: fixed;
  bottom: 20rpx;
  left: 20rpx;
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 400;
  z-index: 999;
}

.addCart_left {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 146rpx;
  height: 100rpx;
  border-radius: 50rpx 4rpx 4rpx 50rpx;
  margin-right: 4rpx;
  color: #fff;
  background: #1a1a1a;
}

.store-box2 {
  width: 100%;
  height: 100%;
}

.addCart_right {
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 560rpx;
  height: 100rpx;
  background: rgba(26, 26, 26, 1);
  border-radius: 4rpx 50rpx 50rpx 4rpx;
}

.add_price {
  margin-left: 25rpx;
  color: #fff;
}

.allSign {
  font-size: 38rpx;
}

.allSign text:nth-child(1) {
  font-size: 34rpx;
}

.allPrice {
  font-size: 22rpx;
  color: #999;
}

.add_text {
  margin-left: 25rpx;
  font-size: 32rpx;
  color: #fff;
}

.btn {
  width: 160rpx;
  height: 62rpx;
  text-align: center;
  line-height: 62rpx;
  border-radius: 31rpx;
  margin-right: 19rpx;
  color: #fff;
  font-size: 28rpx;
}

.store-total-text {
  color: #f1f1f1;
  margin-left: 15rpx;
}
