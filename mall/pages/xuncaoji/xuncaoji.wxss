/* pages/xuncaoji/xuncaoji.wxss */

page {
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  background: #f4edc4;
}

.sologn {
  width: 100%;
  height: 863rpx;
  position: absolute;
  top: 0;
  z-index: -1;
}

.section-swiper {
  width: 710rpx;
  margin: 0 auto;
  margin-top: 20rpx;
}

.section-wrap {
  height: 450rpx;
}

.section-video {
  height: 400rpx;
  width: 710rpx;
}

.section-video video {
  height: 100%;
  width: 100%;
}

.mask_block {
  height: 400rpx;
  width: 710rpx;
  background: #000;
  position: absolute;
  top: 0rpx;
  opacity: 0.2;
  border-radius: 8rpx;
}

.play-img {
  height: 400rpx;
  width: 710rpx;
}

.play {
  position: absolute;
  top: 158rpx;
  left: 297rpx;
  width: 116rpx;
  height: 116rpx;
}

.wrap {
  width: 710rpx;
  background-color: #fff;
  border-radius: 8rpx 8rpx 0 0;
  box-sizing: border-box;
  height: auto;
  margin-left: 20rpx;
  margin-bottom: 40rpx;
  background: #fff;
  padding: 40rpx 0;
}

rich-text {
  display: block;
  width: 650rpx;
  font-size: 28rpx;
  margin: 0 auto;
  letter-spacing: 1rpx;
  font-weight: normal;
}

.block {
  background: #f4edc4;
  height: 30rpx;
  width: 750rpx;
}

/* 分享小图标 */

.small-share {
  width: 108rpx;
  height: 108rpx;
  text-align: center;
  background: #ff2644;
  border-radius: 50%;
  position: fixed;
  bottom: 75rpx;
  right: 20rpx;
}

.small-share image {
  width: 100%;
  height: 100%;
}

/* 分享弹框 */

.commodity_screen1 {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 1);
  opacity: 0.4;
  overflow: hidden;
  z-index: 9999;
  color: rgba(255, 255, 255, 1);
}

/*对话框 */

.commodity_attr_box1 {
  width: 100%;
  overflow: hidden;
  position: fixed;
  bottom: 0;
  left: 0;
  background: rgba(255, 255, 255, 1);
  border-radius: 20rpx 20rpx 0rpx 0rpx;
  z-index: 99999;
}

.content-wrapper1 {
  overflow: hidden;
  height: 414rpx;
  text-align: center;
  position: relative;
}

.content-wrapper1 .cancel {
  width: 44rpx;
  height: 44rpx;
  position: absolute;
  right: 30rpx;
  top: 14rpx;
}

.content-wrapper1 .shareCommission {
  margin-top: 70rpx;
  font-size: 32rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
}

.content-wrapper1 .shareText {
  margin-top: 39rpx;
  font-size: 28rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(102, 102, 102, 1);
}

.content-wrapper1 .share-wrap {
  width: 565rpx;
  margin: 0 auto;
  display: flex;
  justify-content: space-around;
}

.content-wrapper1 .share-button {
  background: rgba(255, 255, 255, 1);
  width: 198rpx;
  border-radius: 0;
}

button::after {
  border: none;
}

.content-wrapper1 .photo {
  margin-top: 30rpx;
  width: 128rpx;
  height: 128rpx;
}

.content-wrapper1 .wx {
  margin-top: -35rpx;
  font-size: 32rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
}

/* 海报弹框 */

.mask_hb {
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.6);
  position: fixed;
  top: 0;
}

.canvas-box {
  width: 0;
  height: 0;
  position: fixed;
  top: 3.9%;
  left: 6%;
}

.canvas {
  width: 88%;
  height: 89%;
  position: fixed;
  top: 3.9%;
  left: 6%;
}

.close_hb {
  width: 100%;
  display: flex;
  justify-content: center;
  position: fixed;
  left: 0;
  bottom: 2%;
}

.close_hb image {
  width: 50rpx;
  height: 50rpx;
}
/* 待返转让弹窗 */

.wait_shure-one-dialog__mask {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.6);
  z-index: 9999;
}

.wait_shure-one-dialog__mask .zan-dialog__container2 {
  width: 630rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 24rpx;
  overflow: hidden;
  position: fixed;
  top: 300rpx;
  left: 60rpx;
  z-index: 9999;
}

.wait_shure-one-dialog__mask .cancel-img {
  width: 30rpx !important;
  height: 30rpx !important;
  position: absolute;
  top: 50rpx;
  right: 50rpx;
}

.wait_shure-one-dialog__mask .zan-dialog__container2 .title1 {
  width: 520rpx;
  margin: 0 auto;
  margin-top: 46rpx;
  margin-bottom: 60rpx;
  font-size: 34rpx;
  font-family: PingFang-SC-Bold;
  font-weight:500;
  color: #222;
  text-align: left;
}

.wait_shure-one-dialog__mask .zan-dialog__container2 .text1 {
  margin: 0 51rpx;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #222;
  line-height: 40rpx;
}
.wait_shure-one-dialog__mask .zan-dialog__container2 .cause{
  letter-spacing: 3rpx;
  font-size: 28rpx;
  font-weight:500;
  color:#333;
  text-align: left !important;
}
.wait_shure-one-dialog__mask .zan-dialog__container2 .wait_name{
  color: #666666 !important;
  letter-spacing: 2rpx;
  position: absolute;
  left: 0;
}
.wait_shure-one-dialog__mask .zan-dialog__container2 .wait_value{
  color: #666666 !important;
  position: absolute;
  right: 40rpx;
}
.wait_shure-one-dialog__mask .zan-dialog__container2 .wait_box{
  position: relative;
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(102,102,102,1);
  height: 40rpx;
  line-height: 40rpx;
}
.wait_shure-one-dialog__mask .zan-dialog__container2 .know{
  margin-top: 40rpx;
  line-height: 76rpx;
  text-align: center;
  display: inline-block;
  font-size: 32rpx;
  color: #fff;
  width:500rpx;
  height:76rpx;
  border:2rpx solid rgba(255,100,23,1);
  background-color: rgba(255,100,23,1);
  border-radius:38rpx;
  /* margin-left: 20rpx; */
}
.wait_shure-one-dialog__mask .zan-dialog__container2 .know_box{
  height: 190rpx;
}
.wait_shure-one-dialog__mask .zan-dialog__container2 .cause_box{
  margin-bottom: 10rpx;
}


