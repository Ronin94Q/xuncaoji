/* 0元购好物 */

.nationwideReturn {
  margin-top: 20rpx;
  background: rgb(255, 255, 255);
}

.nationwideReturn-title {
  position: relative;
  height: 108rpx;
  line-height: 108rpx;
  display: flex;
  align-items: center;
}

.nationwideReturn-price {
  position: absolute;
  right: 26rpx;
  font-size: 26rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
}

.nationwideReturn-price text {
  color: #e33a59;
}

.nationwideReturn-img {
  width: 36rpx;
  height: 36rpx;
  margin: 35rpx 20rpx;
}

.nationwideReturn-text {
  font-size: 34rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: rgba(34, 34, 34, 1);
}

.nationwideReturn-wrapper {
  display: flex;
  flex-wrap: wrap;
  justify-content: flex-start;
  background: rgb(255, 255, 255);
}

.nationwideReturn-list {
  margin-bottom: 20rpx;
  margin-left: 20rpx;
  width: 224rpx;
  position: relative;
}

.nationwideReturn-list-img {
  width: 224rpx;
  height: 224rpx;
  border-radius: 8rpx 8rpx 0rpx 0rpx;
}

.nationwideReturn-list-title {
  display: -webkit-box;
  word-break: break-all;
  text-overflow: ellipsis;
  font-size: 28rpx;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
}

.nationwideReturn-list-price {
  font-size: 20rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(227, 58, 89, 1);
}

.nationwideReturn-list-price text {
  font-size: 16rpx;
}

.nationwideReturn-list-item {
  position: absolute;
  top: 10rpx;
  right: 8rpx;
  width: 106rpx;
  height: 30rpx;
  background: rgba(255, 81, 122, 1);
  opacity: 0.8;
  border-radius: 15rpx;
  font-size: 18rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  line-height: 30rpx;
  text-align: center;
  color: rgba(255, 255, 255, 1);
}

/*超值一口价*/

.goodlist {
  margin-top: 20rpx;
  background: #fff;
  overflow: hidden;
}

.goodlist-title {
  height: 108rpx;
  line-height: 108rpx;
  display: flex;
  align-items: center;
}

.goodlist-img {
  width: 36rpx;
  height: 36rpx;
  margin: 0 20rpx;
  margin-top: 4rpx;
}

.goodlist-text {
  font-size: 34rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: rgba(34, 34, 34, 1);
}

.goodlist-list-scroll {
  white-space: nowrap;
  margin-left: 20rpx;
  display: flex;
  width: 100%;
}

.goodlist-list-item {
  display: inline-block;
}

.goodlist-list-img {
  width: 192rpx;
  height: 192rpx;
  border: 2rpx solid #fff;
  margin-right: 20rpx;
}

.goodlist-list-img image {
  width: 192rpx;
  height: 192rpx;
  border-radius: 8rpx;
}

.goodlist-list-text {
  width: 160rpx;
  height: 44rpx;
  line-height: 44rpx;
  text-align: center;
  margin:0 auto;
  margin-top: 15rpx;
  margin-bottom: 20rpx;
  background: #ff517a;
  border-radius: 22rpx;
  display: flex;
  justify-content: center;
  color: rgba(255, 255, 255, 1);
}

.text {
  font-size: 24rpx;
}

.price-sign {
  font-size: 20rpx;
}

.num {
  font-size: 28rpx;
}

/*口碑爆品榜*/

.trend {
  margin-top: 20rpx;
  background: #fff;
}

.trend-title {
  height: 108rpx;
  line-height: 108rpx;
  display: flex;
  align-items: center;
}

.trend-img {
  width: 36rpx;
  height: 36rpx;
  margin: 0 20rpx;
  margin-top: 4rpx;
}

.trend-text {
  font-size: 34rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: rgba(34, 34, 34, 1);
}

.trend-wrapper {
  overflow: hidden;
  display: flex;
  flex-wrap: wrap;
  padding-bottom: 30rpx;
  justify-content: space-between;
}

.trend-list {
  position: relative;
  width: 365rpx;
  display: flex;
  margin-top: 15rpx;
  padding-bottom: 60rpx;
  background: rgba(248, 248, 248, 1);
}

.trend-list-left .top {
  font-size: 24rpx;
  margin-top: 40rpx;
  margin-left: 20rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  width: 175rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-line-clamp: 1;
  white-space: nowrap;
}

.trend-list-left .bottom {
  margin-top: 19rpx;
  margin-left: 29rpx;
  height: 19prx;
  font-size: 20rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
}

.trend-list-right {
  width: 100rpx;
  height: 100rpx;
  position: absolute;
  right: 26rpx;
  top: 26rpx;
}

.trend-list-right image {
  width: 100rpx;
  height: 100rpx;
  border-radius: 6rpx;
}

/*销量排行榜*/

.quality {
  margin-top: 20rpx;
  overflow: hidden;
}

.quality-title {
  height: 108rpx;
  background: rgba(255, 255, 255, 1);
  line-height: 108rpx;
  display: flex;
  align-items: center;
}

.quality-img {
  width: 36rpx;
  height: 36rpx;
  margin: 0 20rpx;
  margin-top: 4rpx;
}

.quality-text {
  font-size: 34rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: rgba(34, 34, 34, 1);
}

.quality-list-wrapper {
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
}

.quality-list {
  width: 348rpx;
  padding-bottom: 20rpx;
  margin-top: 20rpx;
  margin-left: 18rpx;
  border-radius: 8rpx;
  background: rgba(255, 255, 255, 1);
}

.quality-list-img {
  width: 348rpx;
  height: 348rpx;
}

.quality-list-img image {
  width: 348rpx;
  height: 348rpx;
  border-radius: 8rpx 8rpx 0px 0px;
}

.quality-list-text {
  margin-left: 16rpx;
  margin-top: 10rpx;
  display: -webkit-box;
  word-break: break-all;
  text-overflow: ellipsis;
  font-size: 28rpx;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  color: rgba(51, 51, 51, 1);
}

.quality-list-text text {
  padding: 3rpx 7rpx;
  font-size: 20rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
  background: rgba(255, 81, 122, 1);
  border-radius: 14rpx;
}

.quality-list-price {
  display: flex;
  margin-left: 16rpx;
  margin-top: 30rpx;
}

.newprice {
  font-size: 36rpx;
  color: rgba(227, 58, 89, 1);
}

.newprice text {
  font-size: 24rpx;
}

.oldprice {
  margin-left: 16rpx;
  margin-top: 10rpx;
  font-size: 24rpx;
  text-decoration: line-through;
  color: rgba(153, 153, 153, 1);
}

.return-wrap {
  display: flex;
  height: 40rpx;
  margin-right: 16rpx;
  line-height: 40rpx;
  background: rgba(252, 243, 237, 1);
  border-radius: 8rpx;
  margin-left: 16rpx;
  font-size: 22rpx;
  color: #ff8d12;
}

.return-wrap .return-money text:nth-child(1) {
  color: #999;
  margin-left: 17rpx;
  padding: 0 8rpx;
}

.return-wrap .pay-money text:nth-child(1) {
  color: #999;
  padding: 0 8rpx;
}

.exProducts-bottom {
  font-size: 24rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  text-align: center;
  margin-top: 20rpx;
}
