/* packageA/pages/businessLicence/businessLicence.wxss */
page{
    background-color: #F4F4F4;
    font-family:PingFang SC;
}
.title{
    height: 90rpx;
    margin-left: 30rpx;
    position: relative;
}
.content{
    background-color: #fff;
}
.info{
    font-size:32rpx;
    color:rgba(51,51,51,1);
    height: 90rpx;
    line-height: 90rpx;
}
.commentImg_wrap {
    margin-left: 60rpx;
    margin-top: 66rpx;
    position: relative;
}
.theBusinessImg{
    width: 630rpx;
    height: 410rpx;
    display: inline-block;
}
.commentImg_wrap .close {
    width: 34rpx;
    height: 34rpx;
    position: absolute;
    top: -20rpx;
    right: 40rpx;
}
.defult_box{
    width: 630rpx;
    height: 410rpx;
    margin-left: 60rpx;
    background:rgba(231,231,231,1);
    margin-top: 60rpx;
}
.img_box{
    background-color: #F7F7F7;
}
.defult{
    height: 160rpx;
    width: 160rpx;
    display: inline-block;
    margin-left:235rpx ;
    margin-top: 140rpx;
}
.bottom{
    height: 150rpx;
}
.submit{
    width:690rpx;
    height:80rpx;
    background:linear-gradient(90deg,rgba(255,106,72,1) 0%,rgba(255,38,68,1) 100%);
    border-radius:40rpx;
    font-size:32rpx;
    color:rgba(255,255,255,1);
    line-height: 80rpx;
    text-align: center;
    position: fixed;
    left: 30rpx;
    bottom: 30rpx;
}