/* pages/myorder/myorder.wxss */

page {
  background: #f4f4f4;
}

.nav {
  width: 100%;
  display: flex;
  background: #fff;
  position: fixed;
  top: 0rpx;
  height: 80rpx;
  line-height: 72rpx;
  border-bottom: 2rpx solid #f4f4f4;
  z-index: 999;
  box-sizing: border-box;
  font-size: 30rpx;
  white-space: nowrap;
}

.nav-box {
  display: inline-block;
  padding: 0 20rpx;
}

.nav-item {
  text-align: center;
  color: #555;
}

.nav-item.active {
  color: #ff6417;
}

.nav-item.active1 {
  width: 60rpx;
  height: 6rpx;
  background: #ff6417;
  margin-left: 10rpx;
}

/* 列表 */

.myOrder-wrapper-content {
  overflow: hidden;
  margin-top: 80rpx;
}

.myOrder-wrapper {
  background: rgba(255, 255, 255, 1);
  margin-bottom: 20rpx;
  position: relative;
}

.shop {
  position: relative;
  height: 80rpx;
  line-height: 80rpx;
  border-bottom: 2rpx solid #f4f4f4;
  display: flex;
  font-size: 26rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
}

.goods-channel {
  width: 60rpx;
  height: 28rpx;
  line-height: 28rpx;
  text-align: center;
  background: #ff2644;
  border-radius: 14px;
  font-size: 20rpx;
  color: rgba(255, 255, 255, 1);
  margin-left: 10rpx;
  margin-top: 26rpx;
}

.shop image {
  width: 32rpx;
  height: 32rpx;
  margin-left: 26rpx;
  margin-right: 13rpx;
  margin-top: 25rpx;
  border-radius: 50%;
}

.shop .title {
  color: rgba(102, 102, 102, 1);
}

.shop .state {
  color: #ff2644;
  position: absolute;
  right: 30rpx;
}

.shops-wrap-bottom-wrap {
  display: flex;
  height: 160rpx;
}

.shops-wrap-bottom-img {
  background-image: url('https://xuncj.yzsaas.cn/_download/img/icon/zhanw_tb_n.png');
  background-size: 100% 100%;
  background-repeat: no-repeat;
  width: 120rpx;
  height: 120rpx;
  margin-left: 30rpx;
  margin-top: 24rpx;
  border-radius: 8rpx;
}

.shops-wrap-bottom-img image {
  width: 120rpx;
  height: 120rpx;
}

.shops-wrap-bottom-right {
  position: relative;
  display: flex;
  margin: 0 30rpx;
  margin-top: 20rpx;
  justify-content: space-between;
  width: 570rpx;
}

.shops-wrap-bottom-title {
  width: 310rpx;
  height: 79rpx;
  display: -webkit-box;
  word-break: break-all;
  text-overflow: ellipsis;
  font-size: 28rpx;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  margin-right: 95rpx;
  color: rgba(51, 51, 51, 1);
}

.shops-wrap-bottom-size {
  font-size: 24rpx;
  margin-top: 15rpx;
  color: rgba(153, 153, 153, 1);
}

.size1 {
  margin-left: 20rpx;
}

.shops-wrap-bottom-price {
  font-size: 24rpx;
  color: rgba(153, 153, 153, 1);
}

.price1 {
  font-size: 28rpx;
}

.shops-wrap-bottom-num {
  color: rgba(153, 153, 153, 1);
  position: absolute;
  right: 0rpx;
  top: 83rpx;
}

.shops-wrap-bottom-num text:nth-child(1) {
  padding-right: 8rpx;
  font-size: 28rpx;
}

.shops-wrap-bottom-num text:nth-child(2) {
  padding-right: 2rpx;
  font-size: 26rpx;
}

.all {
  display: flex;
  height: 80rpx;
  line-height: 80rpx;
  justify-content: flex-end;
  margin-right: 30rpx;
  font-size: 26rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(102, 102, 102, 1);
}

.all .num {
  margin-top: 2rpx;
  margin-right: 20rpx;
}

.all .price .sign {
  font-size: 24rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
}

.all .price .text {
  font-size: 32rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
}

.button {
  display: flex;
  padding-bottom: 26rpx;
  justify-content: flex-end;
  margin-right: 10rpx;
}

.button text {
  margin-right: 15rpx;
  height: 56rpx;
  line-height: 56rpx;
  text-align: center;
  background: rgba(255, 255, 255, 1);
  border-radius: 28rpx;
  font-size: 26rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  padding: 0 15rpx;
}

.stopzero {
  /* border: 2rpx solid rgba(153, 153, 153, 1); */
  border: #ff2644 2rpx solid;
  color: #ff2644;
  width: 270rpx;
  padding: 0 5rpx;
  height: 60rpx;
  margin-right: 15rpx;
  border-radius: 28rpx;
  line-height: 60rpx;
  font-size: 26rpx;
  background: #fff;
}

.stopzero[disabled] {
  color: #e9e6e6 !important;
  border: 2rpx solid #e9e6e6 !important;
  background: #fff !important;
}

.cancel {
  border: 2rpx solid rgba(153, 153, 153, 1);
  color: rgba(153, 153, 153, 1);
}

.return {
  border: 2rpx solid rgba(153, 153, 153, 1);
  color: rgba(153, 153, 153, 1);
}

.return-money {
  border: 2rpx solid rgba(153, 153, 153, 1);
  color: rgba(153, 153, 153, 1);
}

.pay {
  border: 2rpx solid #ff2644;
  color: #ff2644;
}

.myorder-bottom {
  font-size: 24rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  text-align: center;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}

.myorder-bottom1 {
