/*对话框 */

.commodity_attr_box1 {
  width: 100%;
  overflow: hidden;
  position: fixed;
  bottom: 0;
  left: 0;
  background: rgba(255, 255, 255, 1);
  border-radius: 20rpx 20rpx 0rpx 0rpx;
  z-index: 99999;
}

.content-wrapper1 {
  overflow: hidden;
  height: auto;
  text-align: center;
