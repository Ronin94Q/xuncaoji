  border-bottom: 2rpx solid #f4f4f4;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: rgba(153, 153, 153, 1);
  padding: 0 30rpx;
}

.cashback-zero .list {
  display: flex;
  height: 90rpx;
