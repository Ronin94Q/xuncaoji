/*弹窗*/

.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 1);
  opacity: 0.5;
}

.modal-dialog { 
  width: 630rpx;
  background: #ff4222;
  background: radial-gradient(circle, rgb(255, 121, 38), rgb(247, 153, 46), rgb(255, 121, 38));
  border-radius: 20rpx;
  overflow: hidden;
  position: fixed;
  top: 28rpx;
  left: 60rpx;
  z-index: 9999;
}

