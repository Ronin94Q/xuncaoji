.del {
  background: #ff2644;
  width: 90px;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  color: #fff;
  -webkit-transform: translateX(90px);
  transform: translateX(90px);
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
}

.touch-move-active .container_cart, .touch-move-active .del {
  -webkit-transform: translateX(0);
  transform: translateX(0);
}

.detail-bottom {
  font-size: 24rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  text-align: center;
  margin-top: 20rpx;
  margin-bottom: 40rpx;
}

/* 底部 */

.store-total {
  display: flex;
  line-height: 90rpx;
  font-size: 30rpx;
  height: 90rpx;
  background: #fff;
  border-top: 2rpx solid #f4f4f4;
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  z-index: 999;
}

.store-total-box {
  margin-left: 25rpx;
  margin-right: 30rpx;
  width: 30rpx;
  height: 30rpx;
}

.store-total-expenses {
  margin-left: 60rpx;
  margin-right: 20rpx;
  font-size: 24rpx;
  color: rgba(136, 136, 136, 1);
}

.store-total-btn {
  position: absolute;
  right: 0;
  bottom: 0;
  width: 180rpx;
  text-align: center;
  color: #fff;
}

.store-total-num {
  width: 180rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  font-size: 28rpx;
  color: #e4405e;
}

.store-total-num-text {
  font-size: 24rpx;
}

