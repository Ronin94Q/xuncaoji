/*搜索历史*/
.wrapper{
  background: #fff;
  height: 1095rpx;
}
.history {
  height: 80rpx;
  width: 690rpx;
  margin: 0 auto;
  margin-top: 108rpx;
  display: flex;
  font-size: 28rpx;
  justify-content: space-between;
  align-items: center;
}

.history-search {
  color: rgba(51, 51, 51, 1);
}

.history-empty {
  color: #FF6417;
}

.content {
  width: 690rpx;
  margin-left: 30rpx;
  display: flex;
  flex-wrap: wrap;
}

.content-list {
  padding: 13rpx 37rpx;
  background: rgba(244, 244, 244, 1);
  border-radius: 25rpx;
  font-size: 24rpx;
  color: rgba(102, 102, 102, 1);
  margin: 20rpx 20rpx;
}
