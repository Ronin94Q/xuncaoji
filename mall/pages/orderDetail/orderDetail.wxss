/* pages/orderDetail/orderDetail.wxss */

page {
  background: #f4f4f4;
}

/* 订单状态 */
.orderDetail-wrapper{
  overflow: hidden;
}
.orderDetail {
  overflow: hidden;
  background: rgb(255, 255, 255);
  margin-bottom: 20rpx;
}

.orderDetail .top {
  position: relative;
  width: 690rpx;
  height: 160rpx;
  background: linear-gradient(65deg, rgba(255, 141, 18, 1) 0%, rgba(255, 170, 77, 1) 100%);
  border-radius: 10rpx;
  margin-left: 30rpx;
  margin-top: 30rpx;
  display: flex;
}

.orderDetail .top .text:nth-child(1) text {
  display: block;
  margin-left: 52rpx;
  margin-top: 64rpx;
  font-size: 34rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: rgba(255, 255, 255, 1);
}

.orderDetail .top .text .text1 {
  display: block;
  margin-left: 52rpx;
  margin-top: 30rpx;
  font-size: 34rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: rgba(255, 255, 255, 1);
}

.orderDetail .top .text .text2 {
  display: block;
  margin-left: 52rpx;
  margin-top: 20rpx;
  font-size: 28rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
}

.orderDetail .top image {
  width: 80rpx;
  height: 80rpx;
  position: absolute;
  right: 52rpx;
  bottom: 40rpx;
}

/*平台保证*/

.platform-list {
  height: 80rpx;
  line-height: 80rpx;
  display: flex;
  padding: 0 30rpx;
}

.platform-list-img {
  width: 24rpx;
  height: 24rpx;
