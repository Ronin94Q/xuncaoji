.quality-list-wrapper {
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
  margin-left: 30rpx;
}

.quality-list {
  width: 339rpx;
  margin-top: 20rpx;
  margin-right: 16rpx;
  border-radius: 8rpx;
  background: rgba(255, 255, 255, 1);
}

.quality-list-img {
  background-image: url('https://xuncaoji.net/_download/img/icon/zhanw_tb_n.png');
  background-size: 100% 100%;
  background-repeat: no-repeat;
  width: 339rpx;
  height: 339rpx;
}

.quality-list-img image {
  width: 339rpx;
  height: 339rpx;
  border-radius: 8rpx 8rpx 0px 0px;
}

.quality-list-text {
  margin-left: 16rpx;
  margin-right: 16rpx;
  margin-top: 10rpx;
  height: 76rpx;
  display: -webkit-box;
  word-break: break-all;
  text-overflow: ellipsis;
  font-size: 28rpx;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  color: rgba(51, 51, 51, 1);
}

.quality-list-text .item1 {
  padding: 2rpx 12rpx;
  margin-right: 13rpx;
  font-size: 20rpx;
  color: rgba(255, 255, 255, 1);
  background: #ff2644;
  border-radius: 14rpx;
}

.quality-list-text .item2 {
  padding: 2rpx 12rpx;
  margin-right: 13rpx;
  font-size: 20rpx;
  color: rgba(255, 100, 23, 1);
  border: 1rpx solid rgba(255, 100, 23, 1);
  border-radius: 14rpx;
}

.quality-list-price {
  display: flex;
  margin-left: 16rpx;
  margin-bottom: 16rpx;
}

.newprice {
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  font-size: 36rpx;
  color: #ff6417;
}

.newtext {
  font-size: 24px;
  font-family: PingFang SC;
  font-weight: 400;
}

.newprice text {
  font-size: 24rpx;
}

.oldprice {
  margin-left: 16rpx;
  margin-top: 10rpx;
  font-size: 24rpx;
  text-decoration: line-through;
  color: rgba(153, 153, 153, 1);
}

.return-wrap {
  display: flex;
  height: 40rpx;
  margin-right: 16rpx;
  line-height: 40rpx;
  background: rgba(252, 243, 237, 1);
  border-radius: 8rpx;
  margin-left: 16rpx;
  font-size: 22rpx;
  color: #ff2644;
  font-family: PingFang-SC-Bold;
  font-weight: Bold;
}

.return-wrap .return-money text:nth-child(1) {
  color: #ff6417;
  padding: 0 8rpx;
}

.return-wrap .pay-money text:nth-child(1) {
  color: #ff6417;
  padding: 0 8rpx;
}
