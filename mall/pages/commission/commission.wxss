}

.list-left-bottom-year {
  font-size: 24rpx;
  color: rgba(153, 153, 153, 1);
  margin-right: 25rpx;
}

.list-right {
  margin-right: 29rpx;
  margin-top: 40rpx;
  color: rgba(227, 58, 89, 1);
}

.list-right1 {
  margin-right: 29rpx;
  margin-top: 40rpx;
