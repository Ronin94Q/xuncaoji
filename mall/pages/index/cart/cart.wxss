/* pages/index/cart/cart.wxss */

page {
  background: #f4f4f4;
}

/* 没有商品 */

.store-container {
  width: 100%;
}

.store-item {
  width: 200rpx;
  height: 200rpx;
  margin: 0 auto;
  margin-top: 152rpx;
}

.store-item image {
  width: 200rpx;
  height: 200rpx;
}

.store-text {
  font-size: 28rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  text-align: center;
}

/* 店铺 */

.store-container-wrapper {
  overflow: hidden;
}

.shop-view-wrapper {
  margin-left: 20rpx;
  width: 710rpx;
  background: #fff;
  box-shadow: 0rpx 0rpx 30rpx 0rpx #eaeaea;
  border-radius: 8rpx;
  margin-top: 18rpx;
}

.shop-view-wrapper-title {
  display: flex;
  height: 90rpx;
  line-height: 90rpx;
  border-bottom: 2rpx solid #f4f4f4;
}

.shop-view-wrapper-title image {
  width: 36rpx;
  height: 36rpx;
  margin-top: 25rpx;
  margin-right: 10rpx;
}

.shop-view-wrapper-title .text {
  font-size: 30rpx;
  color: rgba(51, 51, 51, 1);
}

.shop-view-wrapper-title .text1 {
  padding: 2rpx 12rpx;
  margin-right: 13rpx;
  font-size: 20rpx;
  color: rgba(255, 255, 255, 1);
  background: #ff2644;
  border-radius: 14rpx;
}

.store-box {
  margin-left: 20rpx;
  margin-top: -10rpx;
  margin-right: 10rpx;
}

.goods-view {
  margin-left: 20rpx;
  position: absolute;
  top: 80rpx;
  width: 40rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
}

.goods-view1 {
  width: 40rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
}

.goods-view2 {
  width: 40rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
}

.touch-item {
  display: flex;
  justify-content: space-between;
  border-bottom: 2rpx solid #f4f4f4;
  width: 100%;
  overflow: hidden;
  position: relative;
  margin-right: 10rpx;
  background: rgb(255, 255, 255);
}

.container_cart{
  width: 100%;
  margin-right: 0;
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
  background: #fff;
  -webkit-transform: translateX(90px);
  transform: translateX(90px);
  margin-left: -90px;
  display: flex;
}
checkbox .wx-checkbox-input {
  border-radius: 50%;
  width: 36rpx;
  height: 36rpx;
}

checkbox .wx-checkbox-input.wx-checkbox-input-checked {
  border: none;
  background: #ff2644;
}

checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
  border-radius: 50%;
  width: 36rpx;
  height: 36rpx;
  line-height: 36rpx;
  text-align: center;
  font-size: 30rpx;
  color: #fff;
  background: transparent;
  transform: translate(-50%, -50%) scale(1);
  -webkit-transform: translate(-50%, -50%) scale(1);
}

/* 商品列表 */

.goods-view-wrapper {
  margin-left: 15rpx;
  margin-top: 35rpx;
}

.goods-view-img {
  width: 150rpx;
  height: 150rpx;
  margin-left: 80rpx;
  margin-top: 40rpx;
  margin-right: 20rpx;
}

.goods-view-name {
  width: 428rpx;
  display: -webkit-box;
  word-break: break-all;
  text-overflow: ellipsis;
  font-size: 28rpx;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  margin-right: 30rpx;
  color: rgba(51, 51, 51, 1);
}

.goods-view-spcDesc {
  margin-top: 15rpx;
  font-size: 24rpx;
  color: rgba(153, 153, 153, 1);
}

.goods-view-bottom {
  display: flex;
  justify-content: space-between;
  margin: 16rpx 30rpx;
  margin-left: 0rpx;
  margin-bottom: 0rpx;
}

.goods-view-sign {
  margin-top: 32rpx;
  display: flex;
  font-size: 18rpx;
  color: #ff2644;
}

.goods-view-price {
  font-size: 28rpx;
  margin-top: -10rpx;
  margin-left: 5rpx;
}

.num-item {
  height: 50rpx;
  border: 1rpx solid rgba(170, 170, 170, 1);
  border-radius: 6rpx;
  display: flex;
  margin-top: 8rpx;
}

.num-item1 {
  width: 168rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  background: rgba(189, 189, 189, 1);
  border-radius: 25px;
  font-size: 24rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
}

.num-item2 {
  width: 168rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  background: rgba(189, 189, 189, 1);
  border-radius: 25px;
  font-size: 24rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
}

.num-item text {
  width: 60rpx;
  line-height: 50rpx;
  text-align: center;
}

.num-item input {
  width: 50rpx;
  height: 44rpx;
  margin: 0 auto;
  text-align: center;
  font-size: 32rpx;
  border-left: 2rpx solid #ccc;
  border-right: 2rpx solid #ccc;
}

.zero-wrap {
  height: 102rpx;
  line-height: 102rpx;
  display: flex;
  position: relative;
}

.zero-message {
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(255, 100, 23, 1);
}

