/* 开通会员按钮 */

.button {
  width: 690rpx;
  height: 76rpx;
  background: #ff6417;
  border-radius: 38px;
  margin-top: 80rpx;
  margin-left: 30rpx;
  margin-bottom: 40rpx;
  text-align: center;
  line-height: 76rpx;
  font-size: 32rpx;
  color: rgba(255, 255, 255, 1);
}
.block{
  height: 30rpx;
  background: #f4f4f4;
  width: 750rpx;
}
/* 佣金余额 */

.commission-balance-wrapper {
  background: rgba(255, 255, 255, 1);
  height: 328rpx;
  overflow: hidden;
  margin-bottom: 20rpx;
}

.commission-balance {
  width: 708rpx;
  height: 261rpx;
  margin-left: 31rpx;
  margin-top: 46rpx;
  position: relative;
}

.commission-balance image {
  width: 708rpx;
  height: 261rpx;
}

.balance {
  position: absolute;
  top: 68rpx;
  left: 54rpx;
  color: rgba(255, 255, 255, 1);
}

.balance-money {
  font-size: 32rpx;
}

.balance-num {
  font-size: 50rpx;
  margin-top: 22rpx;
}

/* 佣金明细当余额大于0 */

/* 提现 */

.withdraw-wrap {
  height: 220rpx;
  background: #f4f4f4;
