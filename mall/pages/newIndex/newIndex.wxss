/* 公告 */

.notice-swiper {
  display: flex;
  justify-content: center;
  position: relative;
}

.notice {
  height: 63rpx;
  background: #fff;
  color: #666;
  font-size: 28rpx;
  /* border-top: 2rpx solid rgba(231, 231, 231, 1); */
  width: 655rpx;
  box-shadow: 0rpx 5rpx 10rpx 0rpx rgba(103, 103, 103, 0.1);
  border-radius: 0rpx 0rpx 8rpx 8rpx;
}

.notice-swiper_item {
  font-size: 24rpx;
  font-family: PingFang SC;
  color: rgba(102, 102, 102, 1);
  display: flex;
  align-items: center;
  margin-left: 150rpx;
  height: 63rpx;
  line-height: 63rpx;
  width: 630rpx;
  display: -webkit-box;
  word-break: break-all;
  text-overflow: ellipsis;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
}

.notice-swiper image {
  width: 100rpx;
  height: 22rpx;
  position: absolute;
  left: 67rpx;
  top: 21rpx;
}

.font_two {
  width: 52rpx;
  height: 29rpx;
  font-size: 20rpx;
  font-family: PingFang SC;
  color: rgba(183, 144, 91, 1);
  border: 1rpx solid rgba(183, 144, 91, 1);
  border-radius: 4rpx;
  position: absolute;
  left: 187rpx;
  top: 17rpx;
  text-align: center;
}

/* 类别 */

.section-swiper {
