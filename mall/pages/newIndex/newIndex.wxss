.swiper-wrapper-item {
  height: 240rpx;
  width: 690rpx;
  /* position: absolute;
    top: 188rpx;
    left: 30rpx; */
  border-radius: 8rpx;
  /* box-shadow:0rpx 5rpx 10rpx 0rpx rgba(103,103,103,0.1); */
}

.slide-image {
  height: 240rpx;
  background-size: cover;
  width: 100%;
  border-radius: 8rpx;
}

.swiper-out {
  width: 750rpx;
  height: 660rpx;
  margin-top: 60rpx;
}

.active-item .slide-image {
  box-shadow: 0 5rpx 20rpx 3rpx rgba(0, 0, 0, 0.15);
}

.swp-item {
  width: 400rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  padding-top: 4rpx;
  opacity: 0.6;
}

.active-item {
  opacity: 1;
}

.swiper-container {
  height: 240rpx;
  width: 690rpx;
  position: absolute;
  top: 188rpx;
  left: 30rpx;
  border-radius: 8rpx;
}

.swiper-container .swiper {
  height: 300rpx;
}

.swiper-container .swiper .img {
  width: 100%;
  height: 100%;
}

.swiper-container .dots {
  position: absolute;
  left: 0;
  right: 0;
  bottom: 20rpx;
  display: flex;
  justify-content: center;
}

.swiper-container .dots .dot {
  margin: 0 8rpx;
  width: 10rpx;
  height: 10rpx;
  background: #fff;
  transition: all 0.6s;
  border-radius: 50%;
}

.swiper-container .dots .dot.active {
  width: 26rpx;
  background: #fff;
  border-radius: 8rpx;
}

