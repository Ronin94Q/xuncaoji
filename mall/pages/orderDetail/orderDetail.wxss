/* 订单信息 */

.order-message {
  background: rgb(255, 255, 255);
  padding-bottom: 30rpx;
}

.order-message .title {
  width: 750rpx;
  height: 72rpx;
  line-height: 72rpx;
  font-size: 28rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  margin-left: 30rpx;
}

.order-message .title text {
  padding-left: 26rpx;
  position: relative;
  display: block;
}

.order-message .title text::after {
  content: '';
  display: block;
  width: 6rpx;
  height: 30rpx;
  background-color: #e3b54a;
  position: absolute;
  top: 21rpx;
  left: 0;
  border-radius: 4rpx;
}

.order-message .content {
  margin-left: 30rpx;
}

.order-message .pay-amount {
  position: relative;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 28rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: #999;
}

.order-message .pay-amount text:nth-child(2) {
  position: absolute;
  right: 30rpx;
  color: #666;
}

.order-block {
  height: 120rpx;
  background: #f4f4f4;
}

.stop-wrap {
  display: flex;
