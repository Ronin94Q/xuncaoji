/* pages/forum/forum.wxss */

page {
  background: #fff;
}

/*导航栏*/

.content_top {
  width: 100%;
  background: #fff;
  position: fixed;
  top: 0rpx;
  height: 80rpx;
  line-height: 72rpx;
  border-bottom: 2rpx solid #f4f4f4;
  z-index: 97;
}

.nav {
  width: 80%;
  box-sizing: border-box;
  font-size: 30rpx;
  white-space: nowrap;
}

.nav-box {
  display: inline-block;
  width: 110rpx;
}

.nav-item {
  text-align: center;
  color: #555;
}

.nav-item.active {
  color: #ff6417;
}

.nav-item.active1 {
  width: 60rpx;
  height: 6rpx;
  background: #ff6417;
  margin-left: 25rpx;
}

.content_all {
  width: 150rpx;
  height: 56rpx;
  text-align: center;
  line-height: 56rpx;
  margin-left: 28rpx;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #fff;
  background: rgba(255, 38, 68, 1);
  border-radius: 28rpx;
  position: absolute;
  right: 30rpx;
  top: 10rpx;
}

.block {
  width: 750rpx;
  height: 20rpx;
  margin-top: 80rpx;
  background: #fff;
}

/* 新消息 */

.new_meg {
  display: flex;
  align-items: center;
  width: 750rpx;
  height: 104rpx;
  background: rgba(255, 255, 255, 1);
}

.new_btn {
  width: 260rpx;
  height: 64rpx;
  background: rgba(255, 38, 68, 1);
  border-radius: 32rpx;
  color: #fff;
  text-align: center;
  margin: 0 auto;
  line-height: 64rpx;
  font-size: 32rpx;
  font-family: PingFang SC;
  font-weight: 500;
}

/* 为您推荐 */

.recommend-title {
  height: 80rpx;
  line-height: 80rpx;
  width: 750rpx;
  display: flex;
  justify-content: center;
  position: relative;
  font-size: 34rpx;
  font-family: PingFang SC;
  font-weight: bold;
}

.recommend-left {
  position: absolute;
  left: 203rpx;
  width: 345rpx;
  height: 10rpx;
}

.recommend-left image {
  width: 345rpx;
  height: 10rpx;
}

/* 图片以下 */

.notcontainer {
  display: flex;
  justify-content: center;
  font-size: 24rpx;
  color: #666;
  margin-top: 100rpx;
}

.container {
  border-bottom: 2rpx solid #f4f4f4;
}

.content_box {
  margin: 0 30rpx;
}

.content_box_photo {
  display: flex;
  justify-content: space-between;
  height: 114rpx;
  align-items: center;
}

.content_box_left {
  display: flex;
  align-items: center;
  font-size: 32rpx;
  font-family: PingFang SC;
  font-weight: bold;
}

.content_box_left image {
  background: #e7e7e7;
  border-radius: 50%;
  height: 64rpx;
  width: 64rpx;
  margin-right: 24rpx;
}

.content_box_right {
  display: flex;
  align-items: center;
}

.content_box_right image {
  width: 100rpx;
  height: 50rpx;
}

.content_box_right .follow {
  margin-left: 24rpx;
  width: 100rpx;
  height: 50rpx;
}

.content-text {
  width: 672rpx;
  font-size: 32rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: rgba(102, 102, 102, 1);
  line-height: 48rpx;
}

.content-item {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  text-overflow: ellipsis;
  word-break: break-all;
  overflow: hidden;
}
.content-item1{
  display: -webkit-box;
  -webkit-box-orient: vertical;
  text-overflow: ellipsis;
  word-break: break-all;
  overflow: hidden;
}
.all_text {
  color: #ff6417;
  text-decoration: underline;
}

.content_img {
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
  width: 520rpx;
  margin-top: 30rpx;
}

.content_img image {
  width: 158rpx;
  height: 158rpx;
  margin-right: 10rpx;
  margin-top: 10rpx;
}

