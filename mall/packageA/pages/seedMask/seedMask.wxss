  font-family: PingFang SC;
  font-weight: bold;
  color: #333;
}

.seedMask-saveImg {
  width: 68rpx;
  height: 68rpx;
  margin: 0 30rpx;
}

.seedMask-saveImgs {
  position: absolute;
  width: 118rpx;
  height: 44rpx;
  right: 0;
  top: 0;
}

.seedMask-saveTexts {
  color: #fff;
  font-size: 20rpx;
  position: absolute;
  right: 20rpx;
  top: 10rpx;
}

.seedMask-doBtn {
  display: flex;
  justify-content: space-between;
  margin: 0 30rpx;
}

.buyBtn {
  width: 325rpx;
  height: 78rpx;
  line-height: 78rpx;
  text-align: center;
  background: rgba(255, 251, 246, 1);
  border: 1rpx solid #ff9000;
  color:#ff9000; 
  border-radius: 10rpx;
}

.seedMaskBtn{
  width: 690rpx;
  height: 80rpx;
  background: linear-gradient(90deg, #ff6a48 0%, #ff2644 100%);
  border-radius: 40rpx;
  font-size: 32rpx;
  line-height: 80rpx;
  text-align: center;
  color: #fff;
  margin-left: 30rpx;
  margin-top: 90rpx;
}
button::after {
