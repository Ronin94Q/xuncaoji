/* packageB/pages/takePages/takePages.wxss */

page {
  background: #f4f4f4;
}

/* 店铺信息 */

.wrap_top {
  width: 750rpx;
  overflow: hidden;
  background: linear-gradient(0deg, rgba(255, 38, 68, 0) 0%, rgba(255, 38, 68, 1) 100%);
}

.tp-message {
  width: 690rpx;
  background: #fff;
  border-radius: 20rpx;
  margin: 0 auto;
  padding-bottom: 30rpx;
}

.items {
  width: 100%;
  border-radius: 20rpx;
  display: flex;
  padding-left: 30rpx;
  padding-right: 20rpx;
  margin-bottom: 10rpx;
  box-sizing: border-box;
  background-color: #fff;
  margin-top: 40rpx;
}

.img_wrap {
  background-image: url('https://xuncaoji.net/_download/img/icon/zhanw_tb_n.png');
  background-size: 100% 100%;
  background-repeat: no-repeat;
  width: 170rpx;
  height: 170rpx;
  margin-right: 30rpx;
  border-radius: 20rpx;
}

.img_wrap image {
  width: 170rpx;
  height: 170rpx;
  border-radius: 20rpx;
}

.goodsDetail {
  height: 100%;
  padding-bottom: 30rpx;
  box-sizing: border-box;
}

.goodsName1 {
  width: 630rpx;
  text-align: left;
  padding-top: 30rpx;
  margin-left: 30rpx;
  font-size: 40rpx;
  color: #333;
  font-family: PingFang SC;
  font-weight: bold;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  line-clamp: 1;
  -webkit-box-orient: vertical;
}

.storeGrade {
  display: flex;
  align-items: center;
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #999;
  margin-top: 20rpx;
}

.re-storeGrade {
  display: flex;
  align-items: center;
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #333;
  margin-top: 10rpx;
}

.goodsBottom {
  display: flex;
  justify-content: space-between;
  width: 430rpx;
}

.storeText text {
  font-weight: normal;
}

.stars image {
  width: 24rpx;
  height: 22rpx;
  margin-right: 4rpx;
}

.takeoutT {
  margin-top: 10rpx;
  color: #ff2644;
  font-size: 24rpx;
}

.takeoutT text:nth-child(1) {
  font-weight: bold;
}

.takeoutT text:nth-child(2) {
  font-weight: bold;
  font-size: 36rpx;
}

.goods-btn {
  width: 120rpx;
  height: 56rpx;
  line-height: 56rpx;
  text-align: center;
  color: #fff;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 500;
  background: linear-gradient(0deg, #ff2644 0%, #ff6a48 100%);
  border-radius: 4rpx;
}

.goods-text {
  font-size: 24rpx;
  color: #ff2644;
  margin-top: 5rpx;
}

.goods-text {
  font-size: 24rpx;
  color: #ff2644;
  margin-top: 5rpx;
}

.takeoutName {
  color: #333;
  font-size: 24rpx;
  margin-top: 30rpx;
}

.goods-right {
  display: flex;
  align-items: center;
  flex-direction: column;
  margin-top: 10rpx;
}

.goods-m {
  font-size: 24rpx;
  color: #333;
}

.tp-middle {
  display: flex;
  justify-content: space-between;
  margin: 0 30rpx;
  font-size: 24rpx;
  color: #333;
  height: 65rpx;
}

.tp-address {
  max-width: 375rpx;
}

.tp-dao {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 50rpx;
}

.tp-dao image {
  width: 26rpx;
  height: 32rpx;
  margin-bottom: 4rpx;
}

.tp-btn {
  display: flex;
  margin: 0 30rpx;
  justify-content: space-between;
  margin-top: 40rpx;
}

.tp-takeout {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 308rpx;
  height: 68rpx;
  border: 1rpx solid #ccc;
  border-radius: 10rpx;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #333;
}

.tp-takeout-img {
  width: 34rpx;
  height: 27rpx;
