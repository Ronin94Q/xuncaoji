/* components/seed_toast/seed_toast.wxss */
.bg{
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: rgba(0, 0, 0, 0.6);
    z-index: 9999;
}
.content{
    position: absolute;
    background:rgba(255,255,255,1);
    width: 600rpx;
    border-radius:24rpx;
    top: 220rpx;
    left: 75rpx;
}
.title{
    height: 142rpx;
    line-height: 142rpx;
    text-align: center;
    font-size:34rpx;
    color:rgba(51,51,51,1);
}
.text_one{
    font-size:28rpx;
    color:rgba(51,51,51,1);
    line-height:42rpx;
    padding-left: 51rpx;
    padding-right: 52rpx;
}
.text_two{
    font-size:28rpx;
    color:rgba(51,51,51,1);
    line-height:42rpx;
    padding-left: 51rpx;
    padding-right: 52rpx;
    margin-top: 10rpx;
}
.message{
    font-size:24rpx;
    color:#FF2644;
    line-height:42rpx;
    padding-left: 51rpx;
    padding-right: 51rpx;
    margin-top: 50rpx;
}
.button_box{
    display: flex;
    justify-content: space-between;
    height: 76rpx;
    padding-left: 48rpx;
    padding-right: 52rpx;
    margin-top: 50rpx;
    margin-bottom: 60rpx;
}
.button_one{
    width:240rpx;
    height:75rpx;
    border:0.5px solid rgba(255,38,68,1);
    border-radius:38rpx;
    font-size:32rpx;
    color:rgba(255,38,68,1);
    text-align: center;
    line-height: 75rpx;
}
.button_two{
    width:240rpx;
    height:76rpx;
    background:rgba(255,38,68,1);
    border-radius:38rpx;
    font-size:32rpx;
    color:rgba(255,255,255,1);
    text-align: center;
    line-height: 76rpx;
}
.cancel-get{
    position: absolute;
    top: 146rpx;
    right: 100rpx;
    width: 44rpx;
    height: 74rpx;
}