/* pages/index/cart/cart.wxss */

page {
  background: rgb(255, 255, 255);
}

/* 没有商品 */
.store-container {
  width: 100%;
}

.store-item {
  width: 200rpx;
  height: 200rpx;
  margin: 0 auto;
  margin-top: 152rpx;
}

.store-item image {
  width: 200rpx;
  height: 200rpx;
}

.store-text {
  font-size: 28rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  text-align: center;
}

/* 店铺 */
.store-container-wrapper{
  overflow: hidden;
}
.shop-view-wrapper-title {
  display: flex;
  height: 90rpx;
  line-height: 90rpx;
  border-bottom: 2rpx solid #f4f4f4;
}

.shop-view-wrapper-title image {
  width: 36rpx;
  height: 36rpx;
  margin-top: 25rpx;
  margin-right: 10rpx;
}

.shop-view-wrapper-title text {
  font-size: 30rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
}

.store-box {
  margin-left: 20rpx;
  margin-top: -10rpx;
  margin-right: 10rpx;
}

.goods-view {
  position: absolute;
  top: 80rpx;
  width: 40rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
}

.goods-view1 {
  width: 40rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
}

.goods-view2 {
  width: 40rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
}

.store-box1 {
  width: 100%;
  position: relative;
  display: flex;
  margin-left: 20rpx;
  margin-right: 10rpx;
  padding-bottom: 20rpx;
  background: rgb(255, 255, 255);
  border-bottom: 2rpx solid #f4f4f4;
}

.store-box2 {
  margin-left: 20rpx;
  margin-right: 10rpx;
}

checkbox .wx-checkbox-input {
  border-radius: 50%;
  width: 36rpx;
  height: 36rpx;
}

checkbox .wx-checkbox-input.wx-checkbox-input-checked {
  border: none;
  background: #FF2644;
}

checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
  border-radius: 50%;
  width: 36rpx;
  height: 36rpx;
  line-height: 36rpx;
  text-align: center;
  font-size: 30rpx;
  color: #fff;
  background: transparent;
  transform: translate(-50%, -50%) scale(1);
  -webkit-transform: translate(-50%, -50%) scale(1);
}

/* 商品列表 */

.goods-view-img {
  width: 150rpx;
  height: 150rpx;
  margin-left: 80rpx;
  margin-top: 40rpx;
