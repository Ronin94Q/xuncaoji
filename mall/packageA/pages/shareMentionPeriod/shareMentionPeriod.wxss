.interest-zan-dialog__mask {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: rgba(0, 0, 0, 0.6);
    z-index: 9999;
  }
  
  .interest-zan-dialog__mask .zan-dialog__container {
    width: 600rpx;
    background: rgba(255, 255, 255, 1);
    border-radius: 24rpx;
    overflow: hidden;
    position: fixed;
    top: 220rpx;
    left: 75rpx;
    z-index: 9999;
  }
  
  .interest-zan-dialog__mask .cancel-img {
    width: 44rpx;
    height: 74rpx;
    position: absolute;
    right: 68rpx;
    top: 226rpx;
  }
  
  .interest-zan-dialog__mask .zan-dialog__container .title {
    width: 170rpx;
    margin: 44rpx auto;
    font-size: 34rpx;
    font-family: PingFang-SC-Bold;
    color: rgba(34, 34, 34, 1);
    text-align: center;
  }
  
  .interest-zan-dialog__mask .table {
    width: 500rpx;
    margin: 0 auto;
    border: 2rpx solid #dbdbdb;
    border-bottom: none;
    border-right: none;
    /* margin-bottom: 20rpx; */
  }
  
  .interest-zan-dialog__mask .th {
    font-size: 24rpx;
    width: 250rpx;
    justify-content: center;
    background:rgba(244,244,244,1);
    color: #333;
    display: flex;
    height: 68rpx;
    align-items: center;
    border-right: 2rpx solid #dbdbdb;
  }
  
  .interest-zan-dialog__mask .tr-height {
    height: auto;
    max-height: 420rpx;
    overflow-y: scroll;
    overflow: -Scroll;
    overflow-x: hidden;
  }
  
  .interest-zan-dialog__mask .tr {
    border-bottom: 2rpx solid #dbdbdb;
    color: #333;
    font-size: 20rpx;
    display: flex;
    width: 100%;
    justify-content: center;
    height: 68rpx;
    align-items: center;
  }
  
  .interest-zan-dialog__mask .td {
    width: 250rpx;
    height: 68rpx;
