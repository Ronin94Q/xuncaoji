/* 公告弹窗 */

.show-showNotice {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.6);
  z-index: 999;
}

.show-showNotice-container {
  width: 630rpx;
  max-height: 478rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 28rpx;
  overflow: hidden;
  position: fixed;
  text-align: center;
  top: 300rpx;
  left: 62rpx;
  z-index: 999;
}

.show-showNotice .show-showNotice-container .rule {
  height: 116rpx;
  line-height: 116rpx;
  font-size: 34rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: rgba(34, 34, 34, 1);
}

.show-showNotice .show-showNotice-container .rule_text {
  text-align: left;
  font-size: 28rpx;
  color: #333;
  line-height: 46rpx;
  padding: 0 40rpx;
  height: auto;
  max-height: 310rpx;
  overflow-y: scroll;
  overflow: -Scroll;
  overflow-x: hidden;
}

.show-showNotice .show-showNotice-container .rule_wrap {
  margin-bottom: 20rpx;
}

.show-showNotice .show-showNotice-container .rule_content {
  font-family: PingFang-SC-Medium;
  color: #999;
}

.show-showNotice-cancel {
