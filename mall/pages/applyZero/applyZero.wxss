/* pages/applyZero/applyZero.wxss */

page {
  background: #f4f4f4;
}

.wrapper-top {
  background: #fff;
  font-size: 28rpx;
  height: 453rpx;
}

.spend {
  height: 100rpx;
  line-height: 100rpx;
  display: flex;
  justify-content: space-between;
  padding: 0 30rpx;
  border-bottom: 2rpx solid #f4f4f4;
}

.spend-right {
  display: flex;
  justify-content: flex-end;
}

.spend-right input {
  width: 100rpx;
  height: 100rpx;
  line-height: 100rpx;
  text-align: right;
  color: #aaa;
}

.spend-money {
  margin-right: 30rpx;
  color: #aaa;
}

.platform {
  height: 100rpx;
  line-height: 100rpx;
  display: flex;
  justify-content: space-between;
  padding: 0 30rpx;
  border-bottom: 2rpx solid #f4f4f4;
}

.platform-right {
  display: flex;
  justify-content: flex-end;
}

.platform-right input {
  width: 100rpx;
  height: 100rpx;
  line-height: 100rpx;
  text-align: right;
  color: #aaa;
}

.platform-month {
  margin-right: 30rpx;
  color: #aaa;
}

.interest {
  height: 100rpx;
  line-height: 100rpx;
  display: flex;
  justify-content: space-between;
  padding: 0 30rpx;
  border-bottom: 2rpx solid #f4f4f4;
}

.interest-right {
  display: flex;
  justify-content: flex-end;
  color: #aaa;
}

.count {
  width: 630rpx;
  height: 80rpx;
  margin-top: 30rpx;
  background: #ff2644;
  border-radius: 40rpx;
  font-size: 32rpx;
  margin-bottom: 111rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
}

.wrapper-bottom {
  background: #fff;
  margin-top: 20rpx;
  overflow: hidden;
}

.pay {
  margin: 37rpx 30rpx;
}

.pay-money {
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
}

.pay-money {
  display: flex;
  justify-content: space-between;
}

.pay-money .right {
  color: #ff6417;
}

.pay-num {
  margin-top: 18rpx;
  font-size: 28rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(255, 38, 68, 1);
}

.get-money {
  font-size: 32rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  margin-left: 30rpx;
  padding-bottom: 40rpx;
}

.btn {
  display: flex;
  margin: 40rpx 0;
}

.btn button:nth-child(1) {
  width: 270rpx;
  height: 80rpx;
  line-height: 80rpx;
  margin-left: 30rpx;
  background: rgba(255, 100, 23, 1);
  border-radius: 40rpx;
  font-size: 32rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
}

.btn button:nth-child(2) {
  width: 390rpx;
  height: 80rpx;
  background: rgba(255, 38, 68, 1);
  border-radius: 40rpx;
  margin-right: 30rpx;
  font-size: 32rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
}

.stop {
  display: flex;
  width: 556rpx;
  margin: 0 auto;
  height: 24rpx;
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
}

.stop image {
  margin-top: 5rpx;
  margin-left: 5rpx;
  width: 28rpx;
  height: 28rpx;
}

.block {
  height: 40rpx;
}

/* 想要的分期月数 */

.period-zan-dialog__mask {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.6);
  z-index: 9999;
}

.period-zan-dialog__mask .zan-dialog__container {
  width: 630rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 24rpx;
  overflow: hidden;
  position: fixed;
  top: 300rpx;
  left: 60rpx;
  z-index: 9999;
}

.period-zan-dialog__mask .cancel-img {
  width: 44rpx;
  height: 74rpx;
  position: absolute;
  right: 68rpx;
  top: 226rpx;
}

.period-zan-dialog__mask .zan-dialog__container .title {
  width: 280rpx;
  margin: 44rpx auto;
  font-size: 34rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: rgba(34, 34, 34, 1);
}

.period-zan-dialog__mask .period-month {
  text-align: center;
}

.period-zan-dialog__mask .period-detail {
  height: 80rpx;
  line-height: 80rpx;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(34, 34, 34, 1);
  border-top: 2rpx solid #bdbdbd;
}

.period-zan-dialog__mask .custom {
  color: #aaa;
}

