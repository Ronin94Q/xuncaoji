  height: 28rpx;
  margin-left: 7rpx;
}

.view_myenergy {
  font-size: 30rpx;
  color: #feffff;
  font-family: 'wangte';
  letter-spacing: 4rpx;
  display: flex;
}

.img_energy {
  width: 48rpx;
  height: 85rpx;
}

.view_energy_num {
  font-family: 'wangte';
  color: white;
  font-size: 80rpx;
  display: table-cell;
  vertical-align: bottom;
}

.img_getenergy {
  width: 178rpx;
  height: 78rpx;
}

/* 能量 */

.view_energy {
  height: 200rpx;
  width: 750rpx;
  padding-top: 10rpx;
  display: flex;
  flex-wrap: wrap;
  margin: 0 20rpx;
  padding-bottom: 30px;
  position: absolute;
  top: 130rpx;
  /* margin-top: 30rpx; *//* overflow: hidden; *//* border: 1px solid #4CD964; */
}

.view_energy_list {
  position: relative;
  height: 100rpx;
  width: 116rpx;
  text-align: center;
  /* border: 1px solid #FEFFFF; */
}

@keyframes heart {
  from {
    transform: translate(0, 0);
    /* opacity: 1; */
  }

  to {
    transform: translate(0, 5px);
    /* opacity: 0.8; */
  }
}

@keyframes lines {
  from {
    transform: translate(0px, 0);
  }

  to {
    transform: translate(3px, 0);
  }
}

.energy_title {
  font-size: 32rpx;
  line-height: 98rpx;
  color: #4dbed0;
  font-weight: 800;
  display: block;
  width: 75rpx;
  height: 91rpx;
  text-align: center;
  font-family: 'wangte';
  background-image: url('https://xuncj.yzsaas.cn/_download/img/seed/ic_drop(2).png');
  background-size: 100% 100%;
  overflow:hidden;
   white-space:nowrap;
    text-overflow:ellipsis;
  /* margin: 0 15rpx; */
}

.energy_tips {
  margin-top: -10rpx;
  font-size: 20rpx;
  color: white;
}

.view_collect {
  text-align: center;
}

.img_collect {
  width: 260rpx;
  height: 113rpx;
  /* background: white;
    border-radius: 10%; */
  animation: mycollect 1.0s ease-in-out 0.7s infinite alternate;
}

@keyframes mycollect {
  from {
    transform: translate(-6px, 0);
  }

  to {
    transform: translate(6px, 0);
  }
}

