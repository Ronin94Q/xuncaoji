/* 期望价格弹窗 */

.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 1);
  opacity: 0.5;
}

.modal-dialog {
  width: 600rpx;
  height: 454rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 24rpx;
  overflow: hidden;
  position: fixed;
  top: 15%;
  left: 75rpx;
  z-index: 99;
}

.modal-title {
  margin-top: 54rpx;
  font-size: 32rpx;
  font-weight: bold;
  text-align: center;
  color: rgba(51, 51, 51, 1);
}

.section {
  display: flex;
  align-items: center;
  margin-top: 30rpx;
  margin-left: 50rpx;
  width: 440rpx;
  height: 80rpx;
  border-bottom: 2rpx solid #ddd;
}

.input-price {
  font-size: 32rpx;
  color: rgba(51, 51, 51, 1);
}

.input-text {
  margin-left: 10rpx;
  text-align: left;
  height: 60rpx;
  line-height: 60rpx;
}

.prompt {
  text-align: left;
  margin-left: 50rpx;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 24rpx;
  color: #ff2644;
}

.modal-imgs {
  position: absolute;
  right: 50rpx;
  top: 56rpx;
  width: 30rpx;
  height: 30rpx;
}

.modal-footer {
  display: flex;
  justify-content: space-between;
  margin: 0 30rpx;
}

.btn-cancel {
  width: 240rpx;
  height: 76rpx;
  color: #ff2644;
  line-height: 76rpx;
  font-size: 32rpx;
  border: 1rpx solid #ff2644;
  border-radius: 38rpx;
  text-align: center;
}

.btn-confirm {
  width: 240rpx;
  height: 76rpx;
  background: #ff2644;
  border-radius: 38rpx;
  line-height: 76rpx;
  font-size: 32rpx;
  color: rgba(255, 255, 255, 1);
  text-align: center;
}
.mention_box{
  display: flex;
